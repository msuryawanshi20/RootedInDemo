        <fullName>rstkf__Applied_cannot_be_greater_than_Txn_Amt</fullName>
        <active>true</active>
        <description>Amount Applied cannot be greater than Transaction Amount</description>
        <errorConditionFormula>rstkf__apinvtxn_amtapplmaint__c &gt;  rstkf__apinvtxn_maintamt__c</errorConditionFormula>
        <errorMessage>Amount Applied cannot be greater than Transaction Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstkf__Discount_Amount_Validation</fullName>
        <active>true</active>
        <description>Discount Amount cannot be greater than Transaction Amount</description>
        <errorConditionFormula>rstkf__apinvtxn_disctermamt__c &gt;  rstkf__apinvtxn_maintamt__c</errorConditionFormula>
        <errorMessage>Discount Amount cannot be greater than Transaction Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstkf__Payment_currency_validation</fullName>
        <active>true</active>
        <description>Payment currency should be same as Maintenance currency</description>
        <errorConditionFormula>!ISBLANK(rstkf__apinvtxn_paycurr__c) &amp;&amp; rstkf__apinvtxn_paycurr__c != rstkf__apinvtxn_maintcurr__c</errorConditionFormula>
        <errorMessage>Payment Currency should be same as Transaction Currency</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstkf__Unapplied_Amount_Validation</fullName>
        <active>true</active>
        <description>Amount Unapplied must always equal Transaction Amount - Amount Applied</description>
        <errorConditionFormula>rstkf__apinvtxn_unapplamtmaint__c != rstkf__apinvtxn_maintamt__c - rstkf__apinvtxn_amtapplmaint__c</errorConditionFormula>
        <errorMessage>Amount Unapplied must always equal Transaction Amount - Amount Applied</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstkf__validate_apinvtxn_maintamt</fullName>
        <active>true</active>
        <description>Transaction Total (Txn Currency) cannot be less than Zero</description>
        <errorConditionFormula>!ISBLANK(rstkf__apinvtxn_maintamt__c) &amp;&amp; rstkf__apinvtxn_maintamt__c &lt; 0</errorConditionFormula>
        <errorMessage>Transaction Total (Txn Currency) cannot be less than Zero</errorMessage>
    </validationRules>
