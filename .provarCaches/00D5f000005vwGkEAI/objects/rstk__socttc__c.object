    <validationRules>
        <fullName>rstk__Clause_Content_Required</fullName>
        <active>true</active>
        <errorConditionFormula>LEN(rstk__socttc_clause__c) == 0</errorConditionFormula>
        <errorMessage>Clause Content Is Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Contract_Terms_Template_reqd_in_new_mode</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISNEW(), ISBLANK(rstk__socttc_soctt__c) )</errorConditionFormula>
        <errorMessage>Contract Terms Template is Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Contract_terms_must_belong_to_same_div</fullName>
        <active>true</active>
        <errorConditionFormula>rstk__socttc_soctt__r.rstk__soctt_div__c != rstk__socttc_div__c</errorConditionFormula>
        <errorMessage>Contract Terms Template should belong to the same division</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Division_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(rstk__socttc_div__c)</errorConditionFormula>
        <errorMessage>Sales Division is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Sequence_Required_and_must_greater_than0</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISBLANK(rstk__socttc_seq__c), rstk__socttc_seq__c &lt;= 0)</errorConditionFormula>
        <errorMessage>Sequence is Required and must be Greater than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Title_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(rstk__socttc_title__c)</errorConditionFormula>
        <errorMessage>Clause Title is Required</errorMessage>
    </validationRules>
