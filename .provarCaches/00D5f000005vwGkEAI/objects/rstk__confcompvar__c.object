    <validationRules>
        <fullName>rstk__Boolean_must_be_Selection</fullName>
        <active>true</active>
        <errorConditionFormula>rstk__confcompvar_booleansel__c == true &amp;&amp; !ISPICKVAL( rstk__confcompvar_source__c , &apos;Selection&apos;)</errorConditionFormula>
        <errorMessage>Boolean for Selection Value cannot be set unless Source Value Entity is Selection</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Default_Value</fullName>
        <active>true</active>
        <description>Default Value should be greater than or equal to zero.</description>
        <errorConditionFormula>rstk__confcompvar_defaultval__c != null &amp;&amp;  rstk__confcompvar_defaultval__c &lt; 0</errorConditionFormula>
        <errorMessage>Default Value should be greater than or equal to zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Formula_Variable</fullName>
        <active>true</active>
        <description>Formula Variable is Required and not modifiable , must be two upper case letters (A thru Z).</description>
        <errorConditionFormula>ISBLANK( rstk__confcompvar_variable__c ) || 
 ( NOT( ISNEW() ) &amp;&amp;  ISCHANGED(rstk__confcompvar_variable__c) ) ||
 NOT(REGEX(rstk__confcompvar_variable__c, &apos;[A-Z][A-Z]&apos;) )</errorConditionFormula>
        <errorMessage>Formula Variable is Required and not modifiable , must be two upper case letters (A thru Z).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Reserved_Words</fullName>
        <active>true</active>
        <description>Reserved words &apos;IF&apos; and &apos;OR&apos; cannot be used as a Formula Variable</description>
        <errorConditionFormula>rstk__confcompvar_variable__c = &apos;OR&apos; || rstk__confcompvar_variable__c = &apos;IF&apos;</errorConditionFormula>
        <errorMessage>Reserved words &apos;IF&apos; and &apos;OR&apos; cannot be used as a Formula Variable</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Source_Component</fullName>
        <active>true</active>
        <description>Source Component is Required if Source Value Entity is Selection Component and it should belong Source Selection</description>
        <errorConditionFormula>(ISBLANK(  rstk__confcompvar_sourcecomp__c  ) &amp;&amp;  ISPICKVAL( rstk__confcompvar_source__c , &apos;Selection Component&apos;)) || ( ISPICKVAL( rstk__confcompvar_source__c , &apos;Selection Component&apos;) &amp;&amp; rstk__confcompvar_sourcecomp__r.rstk__confcomp_confsel__c &lt;&gt;  rstk__confcompvar_sourcesel__c)</errorConditionFormula>
        <errorMessage>Source Component is Required if Source Value Entity is Selection Component and it should belong Source Selection</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Source_Component2</fullName>
        <active>true</active>
        <description>Source Component cannot be the same as the Configuration Component</description>
        <errorConditionFormula>!ISBLANK( rstk__confcompvar_confcomp__c ) &amp;&amp; ! ISBLANK( rstk__confcompvar_sourcecomp__c ) &amp;&amp; rstk__confcompvar_confcomp__c = rstk__confcompvar_sourcecomp__c &amp;&amp; !ISPICKVAL( rstk__confcompvar_source__c , &apos;Selection Component Product Price&apos;) &amp;&amp; !ISPICKVAL( rstk__confcompvar_source__c , &apos;Selection Component Specified Price&apos;)</errorConditionFormula>
        <errorMessage>Source Component cannot be the same as the Configuration Component</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Source_Group</fullName>
        <active>true</active>
        <description>Source Group is Required if Source Value Entity is not Order Line Quantity.</description>
        <errorConditionFormula>ISBLANK( rstk__confcompvar_sourcegroup__c ) &amp;&amp; NOT( ISPICKVAL( rstk__confcompvar_source__c , &apos;Order Line Quantity&apos;) )</errorConditionFormula>
        <errorMessage>Source Group is Required if Source Value Entity is not Order Line Quantity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Source_Selection</fullName>
        <active>true</active>
        <description>Source Selection is Required if  Source Value Entity is not Order Line Quantity or Group and it should belong to selected source group.</description>
        <errorConditionFormula>(ISBLANK( rstk__confcompvar_sourcesel__c ) &amp;&amp; NOT( ISPICKVAL( rstk__confcompvar_source__c , &apos;Order Line Quantity&apos;) ) &amp;&amp; NOT( ISPICKVAL( rstk__confcompvar_source__c , &apos;Group&apos;) )) ||
(
   NOT( ISPICKVAL( rstk__confcompvar_source__c , &apos;Order Line Quantity&apos;) ) &amp;&amp; NOT( ISPICKVAL( rstk__confcompvar_source__c , &apos;Group&apos;)) &amp;&amp; rstk__confcompvar_sourcesel__r.rstk__confsel_confgroup__c &lt;&gt;  rstk__confcompvar_sourcegroup__c  
)</errorConditionFormula>
        <errorMessage>Source Selection is Required if  Source Value Entity is not Order Line Quantity or Group and it should belong to selected source group.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Source_Value_Entity</fullName>
        <active>true</active>
        <description>Source Value Entity is Required.</description>
        <errorConditionFormula>ISBLANK( TEXT(rstk__confcompvar_source__c) )</errorConditionFormula>
        <errorMessage>Source Value Entity is Required.</errorMessage>
    </validationRules>
