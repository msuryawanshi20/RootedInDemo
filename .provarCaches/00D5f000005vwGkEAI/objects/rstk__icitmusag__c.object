    <validationRules>
        <fullName>rstk__Fiscal_Year_Range</fullName>
        <active>false</active>
        <description>Invalid Fiscal Year. Value must be between 2000 and 2199</description>
        <errorConditionFormula>rstk__icitmusag_fyr__c &lt; 2000 ||  rstk__icitmusag_fyr__c &gt; 2199</errorConditionFormula>
        <errorMessage>Invalid Fiscal Year. Value must be between 2000 and 2199</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Period_Range</fullName>
        <active>true</active>
        <description>Invalid Period. Value must be between 1 and 13.</description>
        <errorConditionFormula>rstk__icitmusag_per__c &lt; 1 ||  rstk__icitmusag_per__c &gt; 13</errorConditionFormula>
        <errorMessage>Invalid Period. Value must be between 1 and 13.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__item_validation</fullName>
        <active>true</active>
        <description>Division must match Item number Division</description>
        <errorConditionFormula>rstk__icitmusag_item__r.rstk__icitem_div__c != rstk__icitmusag_div__c</errorConditionFormula>
        <errorMessage>Division must match Item number Division</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__prdcost_validation</fullName>
        <active>false</active>
        <description>Period Cost cannot be less than 0</description>
        <errorConditionFormula>rstk__icitmusag_prdcost__c &lt; 0</errorConditionFormula>
        <errorMessage>Period Cost cannot be less than 0</errorMessage>
    </validationRules>
    <validationRules>
