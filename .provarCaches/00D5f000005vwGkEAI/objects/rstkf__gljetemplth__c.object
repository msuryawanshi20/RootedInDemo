    <validationRules>
        <fullName>rstkf__Company_can_not_be_modified</fullName>
        <active>true</active>
        <description>Company can not be modified</description>
        <errorConditionFormula>AND(NOT(ISNEW()), ISCHANGED(rstkf__gljetemplth_glcmp__c) )</errorConditionFormula>
        <errorMessage>Company can not be modified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstkf__Description_can_not_be_Null</fullName>
        <active>true</active>
        <description>Description_can_not_be_Null</description>
        <errorConditionFormula>ISBLANK(TRIM(rstkf__gljetemplth_desc__c))</errorConditionFormula>
        <errorMessage>Please Enter Description</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstkf__Either_Account_Only_or_Amounts_Requir</fullName>
        <active>true</active>
        <description>Either &apos;Account Only&apos; or &apos;Amounts Required&apos; Flag can be Set</description>
        <errorConditionFormula>AND((rstkf__gljetemplth_accountsonly__c = TRUE),( rstkf__gljetemplth_amountsreq__c  = TRUE))</errorConditionFormula>
        <errorMessage>Either &apos;Account Only&apos; or &apos;Amounts Required&apos; Flag can be Set</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstkf__External_Id_can_not_be_modified</fullName>
        <active>true</active>
        <description>External_Id_can_not_be_modified</description>
        <errorConditionFormula>AND(NOT(ISNEW()), ISCHANGED(rstkf__externalid__c) )</errorConditionFormula>
        <errorMessage>External Id can not be modified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstkf__Person_responsible_must_not_be_NULL</fullName>
        <active>true</active>
        <description>Person responsible must not be NULL</description>
        <errorConditionFormula>ISBLANK(rstkf__gljetemplth_owner__c)</errorConditionFormula>
        <errorMessage>Please Enter Person responsible</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstkf__STD_ID_must_be_of_same_Company</fullName>
        <active>true</active>
        <description>STD_ID_must_be_of_same_Company</description>
        <errorConditionFormula>IF(ISBLANK(rstkf__gljetemplth_stdid__c), false, rstkf__gljetemplth_glcmp__c != rstkf__gljetemplth_stdid__r.rstkf__gljestdid_glcmp__c)</errorConditionFormula>
        <errorMessage>STD ID must be of same Company of Header</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstkf__Template_ID_can_not_be_Null</fullName>
        <active>true</active>
        <description>Template ID can not be Null</description>
        <errorConditionFormula>ISBLANK(TRIM(rstkf__gljetemplth_templateid__c))</errorConditionFormula>
        <errorMessage>Pleas Enter Template ID</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstkf__Template_ID_can_not_be_modified</fullName>
        <active>true</active>
        <description>Template ID can not be modified</description>
        <errorConditionFormula>AND(NOT(ISNEW()), ISCHANGED(rstkf__gljetemplth_templateid__c) )</errorConditionFormula>
        <errorMessage>Template ID can not be modified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstkf__Through_Date_must_be_reater_than_From_Da</fullName>
        <active>true</active>
        <description>Through Date must be greater than From Date</description>
        <errorConditionFormula>rstkf__gljetemplth_fromdate__c &gt; rstkf__gljetemplth_thrudate__c</errorConditionFormula>
        <errorMessage>Through Date must be greater than From Date</errorMessage>
    </validationRules>
