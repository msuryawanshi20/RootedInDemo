        <errorConditionFormula>AND(
  NOT(ISNEW()),
  ISCHANGED( rstk__peitem_tracklot_pl__c )
)</errorConditionFormula>
        <errorMessage>The Lot Tracking indicator may not be changed - delete and re-add this item to change the Lot Tracking attribute.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Check_Serial_Item_is_EA_UOM</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
ISPICKVAL(rstk__peitem_tracksrl_pl__c, &apos;Yes&apos;),
 rstk__peitem_enguom__r.rstk__syuom_uom__c &lt;&gt; &apos;EA&apos;
)</errorConditionFormula>
        <errorMessage>UOM must be Each for serialized items.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Check_Serial_Tracking_Change</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
  NOT(ISNEW()),
  ISCHANGED( rstk__peitem_tracksrl_pl__c )
)</errorConditionFormula>
        <errorDisplayField>rstk__peitem_tracksrl_pl__c</errorDisplayField>
        <errorMessage>The Serial Tracking indicator may not be changed - delete and re-add this item to change the Serial Tracking attribute.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Check_for_Purchased_Phantoms</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(rstk__peitem_type_pl__c, &apos;Phantom&apos;),
ISPICKVAL(rstk__peitem_src_pl__c, &apos;Purchased&apos;))</errorConditionFormula>
        <errorMessage>Eng Type cannot be Phantom when Inventory Source is Purchased.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Commodity_Code_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( rstk__peitem_iccomcod__c )</errorConditionFormula>
        <errorMessage>Commodity Code is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Company_Division</fullName>
        <active>true</active>
        <description>Division should match Engineering Division from Company.</description>
        <errorConditionFormula>NOT(CONTAINS(rstk__triggeroptions__c+&apos; &apos;, &apos;UT&apos;)) &amp;&amp; rstk__peitem_div__r.rstk__sydiv_cmpno__r.rstk__sycmp_engdivind__c &amp;&amp;  rstk__peitem_div__c !=  rstk__peitem_div__r.rstk__sydiv_cmpno__r.rstk__sycmp_engdiv__c</errorConditionFormula>
        <errorMessage>Division should match Engineering Division from Company.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Division_InvDivision</fullName>
        <active>true</active>
        <description>Division should match Inventory Division when not using Centralized Engineering.</description>
        <errorConditionFormula>NOT(CONTAINS(rstk__triggeroptions__c+&apos; &apos;, &apos;UT&apos;)) &amp;&amp; !rstk__peitem_div__r.rstk__sydiv_cmpno__r.rstk__sycmp_engdivind__c &amp;&amp;  rstk__peitem_div__c !=  rstk__peitem_invdiv__c</errorConditionFormula>
        <errorMessage>Division should match Inventory Division when not using Centralized Engineering.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Eng_Type_Values</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR(ISPICKVAL( rstk__peitem_type_pl__c , &apos;Reference&apos;), ISPICKVAL( rstk__peitem_type_pl__c, &apos;Phantom&apos;), ISPICKVAL( rstk__peitem_type_pl__c, &apos;Standard&apos;)))</errorConditionFormula>
        <errorMessage>Status must be Standard, Reference or Phantom.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Inv_Div_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( rstk__peitem_invdiv__c)</errorConditionFormula>
        <errorMessage>Inventory Division is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Inv_and_NNINV_Acct_same_for_FIFO</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   rstk__peitem_fifoind__c,
    VLOOKUP( $ObjectType.rstk__iccomcodacc__c.Fields.rstk__iccomcodacc_invacct__c ,  $ObjectType.rstk__iccomcodacc__c.Fields.Name ,  rstk__peitem_iccomcod__c ) &lt;&gt;
    VLOOKUP(  $ObjectType.rstk__iccomcodacc__c.Fields.rstk__iccomcodacc_invnnacct__c  ,  $ObjectType.rstk__iccomcodacc__c.Fields.Name ,  rstk__peitem_iccomcod__c ) 

)</errorConditionFormula>
        <errorMessage>Under FIFO Cost Method, Inventory and Non-nettable accounts must be the same.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Item_Type_Values</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR( ISBLANK(TEXT(rstk__peitem_itemtype__c)), ISPICKVAL( rstk__peitem_itemtype__c , &apos;Direct Material&apos;), ISPICKVAL( rstk__peitem_itemtype__c , &apos;Labor Only&apos;) ))</errorConditionFormula>
        <errorMessage>Item Type must be blank, Direct Material, or Labor Only</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Lot_Tracking_Values</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR(ISPICKVAL( rstk__peitem_tracklot_pl__c , &apos;No&apos;), ISPICKVAL( rstk__peitem_tracklot_pl__c, &apos;Yes&apos;)))</errorConditionFormula>
        <errorDisplayField>rstk__peitem_tracksrl_pl__c</errorDisplayField>
        <errorMessage>Lot Tracking indicator must be No or Yes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Main_Site_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(rstk__peitem_invdiv__r.rstk__sydiv_mainsite__c)</errorConditionFormula>
        <errorMessage>Main Site must be set on Inventory Division record before adding new items.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Serial_Tracking_Values</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR(ISPICKVAL( rstk__peitem_tracksrl_pl__c , &apos;No&apos;), ISPICKVAL( rstk__peitem_tracksrl_pl__c, &apos;Yes&apos;)))</errorConditionFormula>
        <errorDisplayField>rstk__peitem_tracksrl_pl__c</errorDisplayField>
        <errorMessage>Serial Tracking indicator must be No or Yes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Source_Values</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR(ISPICKVAL(  rstk__peitem_src_pl__c  , &apos;Manufactured&apos;), ISPICKVAL( rstk__peitem_src_pl__c , &apos;Purchased&apos;), ISPICKVAL( rstk__peitem_src_pl__c , &apos;Subcontract&apos;) ))</errorConditionFormula>
        <errorMessage>Source must be Manufactured, Purchased or Subcontract.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Status_Values</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR(ISPICKVAL(   rstk__peitem_status_pl__c   , &apos;Pending&apos;), ISPICKVAL( rstk__peitem_status_pl__c, &apos;Active&apos;)))</errorConditionFormula>
        <errorMessage>Status must be Pending or Active.</errorMessage>
    </validationRules>
