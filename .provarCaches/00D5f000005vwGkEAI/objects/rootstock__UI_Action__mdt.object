    <validationRules>
        <fullName>rootstock__Action_cannot_be_overridden</fullName>
        <active>true</active>
        <description>Checks if the user is allowed to override the action&apos;s field values.</description>
        <errorConditionFormula>IF(rootstock__Allow_Override__c, false, NOT(rootstock__Active__c))</errorConditionFormula>
        <errorMessage>This action cannot be overridden</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rootstock__Toast_type_cannot_be_set</fullName>
        <active>true</active>
        <description>Action value must be &apos;Show Toast&apos; to set toast type.</description>
        <errorConditionFormula>ISPICKVAL(rootstock__Toast_Type__c, &apos;&apos;) == false &amp;&amp; 
ISPICKVAL(rootstock__Action__c, &apos;Show Toast&apos;) == false</errorConditionFormula>
        <errorDisplayField>rootstock__Toast_Type__c</errorDisplayField>
        <errorMessage>Action value must be &apos;Show Toast&apos;</errorMessage>
    </validationRules>
