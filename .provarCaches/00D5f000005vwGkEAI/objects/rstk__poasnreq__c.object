    <validationRules>
        <fullName>rstk__Lot_Expiration_Date</fullName>
        <active>true</active>
        <description>Lot Expiration Date is Required</description>
        <errorConditionFormula>ISBLANK(rstk__poasnreq_expdate__c ) &amp;&amp; ISPICKVAL(rstk__poasnreq_supreq__r.rstk__supreq_icitem__r.rstk__icitem_item__r.rstk__peitem_tracklot_pl__c , &apos;Yes&apos;)</errorConditionFormula>
        <errorMessage>Lot Expiration Date is Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Lot_Number</fullName>
        <active>true</active>
        <description>Lot Numbers are Required</description>
        <errorConditionFormula>ISBLANK(rstk__poasnreq_lotnos__c) &amp;&amp;   ISPICKVAL(rstk__poasnreq_supreq__r.rstk__supreq_icitem__r.rstk__icitem_item__r.rstk__peitem_tracklot_pl__c , &apos;Yes&apos;)</errorConditionFormula>
        <errorMessage>Lot Numbers are Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Requisition</fullName>
        <active>true</active>
        <description>Requisition is Required. Also Requisition Supplier and Division must match the Advance Shipment Notification Supplier and Division.</description>
        <errorConditionFormula>ISBLANK(rstk__poasnreq_supreq__c) ||  (rstk__poasnreq_poasn__r.rstk__poasn_povend__c !=  rstk__poasnreq_supreq__r.rstk__supreq_povend__c) ||  (rstk__poasnreq_poasn__r.rstk__poasn_div__c !=  rstk__poasnreq_supreq__r.rstk__supreq_div__c)</errorConditionFormula>
        <errorMessage>Requisition is Required also Requisition Supplier and Division should be equal to  Advance Shipment Notification Supplier and Division</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Shipment_Quantity</fullName>
        <active>true</active>
        <description>Shipment Quantity is Required and cannot be less than Requisition Quantity.</description>
        <errorConditionFormula>ISBLANK(rstk__poasnreq_qtyship__c) || rstk__poasnreq_qtyship__c &lt; rstk__poasnreq_supreq__r.rstk__supreq_qtyreq__c</errorConditionFormula>
        <errorMessage>Shipment Quantity is Required and cannot be less than Requisition Quantity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__supreq_validation</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW() &amp;&amp;  !rstk__poasnreq_supreq__r.rstk__supreq_accepted__c</errorConditionFormula>
        <errorMessage>Specified Requisition has not been accepted.</errorMessage>
    </validationRules>
