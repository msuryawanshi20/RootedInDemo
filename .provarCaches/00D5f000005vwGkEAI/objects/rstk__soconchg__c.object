    <validationRules>
        <fullName>rstk__Billable_After_This_Date</fullName>
        <active>true</active>
        <description>Billable After This Date  field is Required.</description>
        <errorConditionFormula>ISBLANK(rstk__soconchg_effdate__c)</errorConditionFormula>
        <errorMessage>Billable After This Date  is Required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Charge_Amount</fullName>
        <active>true</active>
        <description>Charge Amount is Required and must be greater than zero.</description>
        <errorConditionFormula>ISBLANK(rstk__soconchg_amount__c ) || rstk__soconchg_amount__c  &lt;= 0</errorConditionFormula>
        <errorMessage>Charge Amount is Required and must be greater than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Charge_Number</fullName>
        <active>true</active>
        <description>Charge Number is Required and not modifiable.</description>
        <errorConditionFormula>(!ISNEW() &amp;&amp; ISCHANGED(rstk__soconchg_seq__c ))</errorConditionFormula>
        <errorMessage>Charge Number is Required and not modifiable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Charge_Number_required_and_not_modifiabl</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(rstk__soconchg_seq__c) || (!ISNEW() &amp;&amp;  ISCHANGED(rstk__soconchg_seq__c) )</errorConditionFormula>
        <errorMessage>Charge Number is required and cannot be modified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Contract_General_Misc_Charge</fullName>
        <active>true</active>
        <description>Contract General Misc Charge is not Modifiable.</description>
        <errorConditionFormula>(!ISNEW() &amp;&amp; ISCHANGED(rstk__soconchg_general__c) )</errorConditionFormula>
        <errorMessage>Contract General Misc Charge is not Modifiable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Contract_Master</fullName>
        <active>true</active>
        <description>Contract is Required and not modifiable.</description>
        <errorConditionFormula>ISBLANK(rstk__soconchg_socontract__c) || ( !ISNEW() &amp;&amp;  ISCHANGED(rstk__soconchg_socontract__c) )</errorConditionFormula>
        <errorMessage>Contract is Required and not modifiable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Covered_Serialized_Device</fullName>
        <active>true</active>
        <description>Covered Serialized Device is Required , not Modifiable and should belong to Contract.</description>
        <errorConditionFormula>(ISBLANK(rstk__soconchg_socondev__c) &amp;&amp; !rstk__soconchg_general__c) ||  (! ISNEW() &amp;&amp;  ISCHANGED(rstk__soconchg_socondev__c) ) || ( !ISBLANK(rstk__soconchg_socondev__c) &amp;&amp;  rstk__soconchg_socondev__r.rstk__socondev_socontract__c &lt;&gt;  rstk__soconchg_socontract__c )</errorConditionFormula>
        <errorMessage>Covered Serialized Device is Required , not Modifiable and should belong to Contract.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Effective_Date_required</fullName>
        <active>true</active>
        <description>Effective Date required</description>
        <errorConditionFormula>ISBLANK(rstk__soconchg_effdate__c)</errorConditionFormula>
        <errorMessage>Effective Date is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Misc_Chrg_Cant_Be_Edited</fullName>
        <active>false</active>
        <description>Miscelleneous charge cannot be edit if Date Included in Periodic Billing Field is specified</description>
        <errorConditionFormula>(!ISNEW() &amp;&amp;  !ISBLANK( PRIORVALUE(rstk__soconchg_billdate__c)))</errorConditionFormula>
        <errorDisplayField>rstk__soconchg_effdate__c</errorDisplayField>
        <errorMessage>Miscelleneous charge cannot be edit if Date Included in Periodic Billing Field is specified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Miscellaneous_Charge_Product</fullName>
        <active>true</active>
        <description>Miscellaneous Charge Product is Required , not modifiable and should belong to Contract Division with  Miscellaneous or Recurring Service Product Type.</description>
        <errorConditionFormula>ISBLANK(rstk__soconchg_soprod__c) ||  ( !ISNEW() &amp;&amp; ISCHANGED(rstk__soconchg_soprod__c)) || ( rstk__soconchg_soprod__r.rstk__soprod_div__c  &lt;&gt;  rstk__soconchg_socontract__r.rstk__socontract_div__c ) || (   TEXT(rstk__soconchg_soprod__r.rstk__soprod_prodtype__c) &lt;&gt; &apos;Miscellaneous&apos;  &amp;&amp;  IF(TEXT(rstk__soconchg_soprod__r.rstk__soprod_prodtype__c) = &apos;Recurring Service&apos; &amp;&amp; (rstk__soconchg_extcomment__c = &apos;Early Termination Charges&apos; || rstk__soconchg_extcomment__c = &apos;Usage Charges&apos;), false, true) )</errorConditionFormula>
        <errorMessage>Miscellaneous Charge Product is Required , not modifiable and should belong to Contract Division with  Miscellaneous or Recurring Service Product Type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Revenue_Account</fullName>
        <active>true</active>
        <description>Revenue Account is Required should belong to Miscellaneous Charge Product division , with GLType Revenue.</description>
        <errorConditionFormula>ISBLANK(rstk__soconchg_salesacct__c) || (  rstk__soconchg_salesacct__r.rstk__syacc_div__c &lt;&gt;  rstk__soconchg_soprod__r.rstk__soprod_div__c ) || ( TEXT( rstk__soconchg_salesacct__r.rstk__syacc_gltype_pl__c ) &lt;&gt; &apos;Revenue&apos; )</errorConditionFormula>
        <errorMessage>Revenue Account is Required should belong to Miscellaneous Charge Product division , with GLType Revenue.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Tax_Rate</fullName>
        <active>true</active>
        <description>Tax Rate cannot be less than zero and not Modifiable as Tax is already Verified.</description>
        <errorConditionFormula>(rstk__soconchg_taxrate__c &lt; 0 )|| ( ISCHANGED(rstk__soconchg_taxrate__c) &amp;&amp;  rstk__soconchg_taxverified__c )</errorConditionFormula>
        <errorMessage>Tax Rate cannot be less than zero and not Modifiable as Tax is already Verified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Usage_Amount_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>(!ISBLANK(rstk__soconchg_usage1chg__c) &amp;&amp; rstk__soconchg_usage1chg__c &lt; 0)  || (!ISBLANK(rstk__soconchg_usage2chg__c) &amp;&amp; rstk__soconchg_usage2chg__c &lt; 0) || (!ISBLANK(rstk__soconchg_usage3chg__c) &amp;&amp; rstk__soconchg_usage3chg__c &lt; 0) || (!ISBLANK(rstk__soconchg_usage4chg__c) &amp;&amp; rstk__soconchg_usage4chg__c &lt; 0) || (!ISBLANK(rstk__soconchg_usage5chg__c) &amp;&amp; rstk__soconchg_usage5chg__c &lt; 0) || (!ISBLANK(rstk__soconchg_usage6chg__c) &amp;&amp; rstk__soconchg_usage6chg__c &lt; 0)</errorConditionFormula>
        <errorMessage>Usage Amount cannot be Less than zero</errorMessage>
    </validationRules>
