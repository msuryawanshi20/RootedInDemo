        <errorMessage>Quantity is Required and must be greater than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Sales_Customer_ShipTo_Address</fullName>
        <active>true</active>
        <description>Sales Customer Ship-To Address is Required and should belong to Contract Customer Number.</description>
        <errorConditionFormula>ISBLANK(rstk__soconppsched_staddr__c) || ( !ISBLANK(rstk__soconppsched_upgroup__c ) &amp;&amp;  ISCHANGED(rstk__soconppsched_staddr__c)  ) ||  (rstk__soconppsched_staddr__r.rstk__socaddr_custno__c &lt;&gt;  rstk__soconppsched_soconpp__r.rstk__soconpp_socontract__r.rstk__socontract_sohdrcust__c ) || (!rstk__soconppsched_staddr__r.rstk__socaddr_useasshipto__c )</errorConditionFormula>
        <errorMessage>Sales Customer Ship-To Address is Required and should belong to Contract Customer Number.</errorMessage>
    </validationRules>
