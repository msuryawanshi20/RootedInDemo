    <validationRules>
        <fullName>rstk__Non_negative_LT_Offset</fullName>
        <active>true</active>
        <errorConditionFormula>rstk__pebomovr_ltoffset__c &lt;0</errorConditionFormula>
        <errorMessage>Please enter a non-negative Lead Time Offset.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Non_negative_Setup_Qty</fullName>
        <active>true</active>
        <errorConditionFormula>rstk__pebomovr_setupqty__c  &lt; 0</errorConditionFormula>
        <errorMessage>Please enter a non-negative Setup Quantity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Process_No_Division</fullName>
        <active>true</active>
        <description>Process No Division must match Inventory Division</description>
        <errorConditionFormula>rstk__pebomovr_procno__c != null &amp;&amp; rstk__pebomovr_procno__r.rstk__rtproc_div__c != rstk__pebomovr_invdiv__c</errorConditionFormula>
        <errorMessage>Process No Division must match Inventory Division</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Scrap_Pcnt</fullName>
        <active>true</active>
        <errorConditionFormula>rstk__pebomovr_scrappcnt__c &lt;0 || rstk__pebomovr_scrappcnt__c &gt;=1</errorConditionFormula>
        <errorMessage>Please enter a Scrap Factor greater than or equal to 0 and less than 1.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Work_Center_Division</fullName>
        <active>true</active>
        <description>Work Center Division must match Inventory Division</description>
        <errorConditionFormula>rstk__pebomovr_wctr__c != null &amp;&amp;  rstk__pebomovr_wctr__r.rstk__rtwctr_div__c !=  rstk__pebomovr_invdiv__c</errorConditionFormula>
        <errorMessage>Work Center Division must match Inventory Division</errorMessage>
    </validationRules>
