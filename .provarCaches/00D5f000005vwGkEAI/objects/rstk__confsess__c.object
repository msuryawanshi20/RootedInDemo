    <validationRules>
        <fullName>rstk__Validate_Discount_Method_1</fullName>
        <active>true</active>
        <description>Discount Percentage must be specified</description>
        <errorConditionFormula>ISPICKVAL( rstk__confsess_discmethod__c , &apos;Create Order Line for Discount&apos;) &amp;&amp;  rstk__confsess_discpct__c == 0</errorConditionFormula>
        <errorMessage>Discount Percentage must be specified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Discount_Method_2</fullName>
        <active>true</active>
        <description>Invalid Value for Configuration Product</description>
        <errorConditionFormula>ISPICKVAL( rstk__confsess_discmethod__c , &apos;Create Order Line for Discount&apos;) &amp;&amp;  ISBLANK(rstk__confsess_soprod__r.rstk__soprod_confdiscprod__c)</errorConditionFormula>
        <errorMessage>Invalid Value for Configuration Product</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Discount_Percentage</fullName>
        <active>true</active>
        <description>Discount Percentage must be between 0 and 100</description>
        <errorConditionFormula>rstk__confsess_discpct__c &lt; 0 || rstk__confsess_discpct__c &gt; 100</errorConditionFormula>
        <errorMessage>Discount Percentage must be between 0 and 100</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Icitem</fullName>
        <active>true</active>
        <description>Manufactured Item is required when Export Features to Serialized Item is checked</description>
        <errorConditionFormula>rstk__confsess_expicitemsrl__c = true &amp;&amp;  rstk__confsess_icitem__c = null</errorConditionFormula>
        <errorMessage>Manufactured Item is required when Export Features to Serialized Item is checked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Icitemsrl</fullName>
        <active>true</active>
        <description>Inventory By Serial Number is required when Export Features to Serialized Item is checked</description>
        <errorConditionFormula>rstk__confsess_expicitemsrl__c = true &amp;&amp;  rstk__confsess_icitemsrl__c = null</errorConditionFormula>
        <errorMessage>Inventory By Serial Number is required when Export Features to Serialized Item is checked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Previous_Confsess_Division</fullName>
        <active>true</active>
        <description>Previous Configuration Session Division must match Configuration Division</description>
        <errorConditionFormula>rstk__confsess_confsess__c != null &amp;&amp;  rstk__confsess_confsess__r.rstk__confsess_div__c !=  rstk__confsess_div__c</errorConditionFormula>
        <errorMessage>Previous Configuration Session Division must match Configuration Division</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Price</fullName>
        <active>true</active>
        <description>Price cannot be less than zero</description>
        <errorConditionFormula>rstk__confsess_price__c &lt; 0</errorConditionFormula>
        <errorMessage>Price cannot be less than zero</errorMessage>
    </validationRules>
