    <fields>
        <fullName>rstk__soconrc_billpdprice__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>CASE(rstk__soconrc_priceuom__c, 

     &apos;Monthly&apos;, CASE(rstk__soconrc_socontract__r.rstk__socontract_rcbfreq__c, 

				&apos;Monthly&apos;, rstk__soconrc_price__c ,

				&apos;Quarterly&apos;, rstk__soconrc_price__c * 3,

				&apos;Semi-Annual&apos;,rstk__soconrc_price__c * 6,
    
				&apos;Annual&apos;,rstk__soconrc_price__c * 12,

      
		 0),


     &apos;Quarterly&apos;, CASE(rstk__soconrc_socontract__r.rstk__socontract_rcbfreq__c, 

				&apos;Monthly&apos;, rstk__soconrc_price__c / 3,

				&apos;Quarterly&apos;, rstk__soconrc_price__c,

				&apos;Semi-Annual&apos;,rstk__soconrc_price__c * 2,
    
				&apos;Annual&apos;,rstk__soconrc_price__c * 4,

      
		 0),


     &apos;Semi-Annual&apos;,CASE(rstk__soconrc_socontract__r.rstk__socontract_rcbfreq__c, 

				&apos;Monthly&apos;, rstk__soconrc_price__c / 6,

				&apos;Quarterly&apos;, rstk__soconrc_price__c / 2,

				&apos;Semi-Annual&apos;,rstk__soconrc_price__c,
    
				&apos;Annual&apos;,rstk__soconrc_price__c * 2,

      
		 0),

    
     &apos;Annual&apos;,CASE(rstk__soconrc_socontract__r.rstk__socontract_rcbfreq__c, 

				&apos;Monthly&apos;, rstk__soconrc_price__c / 12,

				&apos;Quarterly&apos;, rstk__soconrc_price__c / 4,

				&apos;Semi-Annual&apos;,rstk__soconrc_price__c /2,
    
				&apos;Annual&apos;,rstk__soconrc_price__c,

      
		 0),

      
     0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>The charge for the Billing Period defined (the charge per Billing Period).</inlineHelpText>
        <label>Billing Period Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
