    <validationRules>
        <fullName>rstk__Cant_change_Apply_Number_of_Billing_Peri</fullName>
        <active>true</active>
        <errorConditionFormula>(rstk__soconrc_billed__c &gt; 0) &amp;&amp; (rstk__soconrc_deviceterm__c !=  PRIORVALUE(rstk__soconrc_deviceterm__c) )</errorConditionFormula>
        <errorMessage>Apply Number of Billing Periods by Device cannot be changed if Number of Periods Billed is greater than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Charge_Application_Method</fullName>
        <active>true</active>
        <description>Charge Application Method cannot be blank.</description>
        <errorConditionFormula>ISBLANK(TEXT(rstk__soconrc_application__c))</errorConditionFormula>
        <errorMessage>Charge Application Method cannot be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Cont_Billing_Until_Device_is_Deactivated</fullName>
        <active>true</active>
        <description>Continue Billing Until Device is Deactivated field cannot be checked if Continue Billing When Term Expires field is unchecked</description>
        <errorConditionFormula>rstk__soconrc_forever__c &amp;&amp; !rstk__soconrc_overterm__c</errorConditionFormula>
        <errorMessage>Continue Billing Until Device is Deactivated field cannot be checked if Continue Billing When Term Expires field is unchecked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Cont_Billing_When_Term_Expires_condition</fullName>
        <active>true</active>
        <description>Continue Billing When Term Expires must be checked when Apply Number of Billing Periods by Device is checked</description>
        <errorConditionFormula>rstk__soconrc_deviceterm__c &amp;&amp; !rstk__soconrc_overterm__c</errorConditionFormula>
        <errorMessage>Continue Billing When Term Expires must be checked when Apply Number of Billing Periods by Device is checked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Continue_Billing_When_Term_Expires</fullName>
        <active>true</active>
        <description>Cannot change Continue Billing When Term Expires Field as Number of Periods Billed is more than Number of Periods to Bill .</description>
        <errorConditionFormula>!rstk__soconrc_overterm__c &amp;&amp;  PRIORVALUE(rstk__soconrc_overterm__c) &amp;&amp; (rstk__soconrc_billed__c &gt; rstk__soconrc_term__c )</errorConditionFormula>
        <errorMessage>Cannot change Continue Billing When Term Expires Field as Number of Periods Billed is more than Number of Periods to Bill .</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Covered_Device_Inventory_Item</fullName>
        <active>true</active>
        <description>Covered Device Inventory Item is required if Application is Applies to a Specific Item else not required.</description>
        <errorConditionFormula>((TEXT(rstk__soconrc_application__c) == &apos;Applies to a Specific Item&apos;) &amp;&amp; ISBLANK(rstk__soconrc_devicitem__c)) || (!ISBLANK(rstk__soconrc_devicitem__c) &amp;&amp; (TEXT(rstk__soconrc_application__c) != &apos;Applies to a Specific Item&apos;) )</errorConditionFormula>
        <errorMessage>Covered Device Inventory Item is required if Application is Applies to a Specific Item else not required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Early_Termination_Fee_Pct</fullName>
        <active>true</active>
        <description>Early Termination Fee Pct cannot be less than zero.</description>
        <errorConditionFormula>!ISBLANK( rstk__soconrc_earlyexitpct__c )  &amp;&amp;  rstk__soconrc_earlyexitpct__c &lt; 0</errorConditionFormula>
        <errorMessage>Early Termination Fee Pct cannot be less than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Mfg_Account</fullName>
        <active>true</active>
        <description>Mfg Account is required and should belong to Recurring Charge Product Division.</description>
        <errorConditionFormula>ISBLANK(rstk__soconrc_salesacct__c) ||  (rstk__soconrc_salesacct__r.rstk__syacc_div__c &lt;&gt;  rstk__soconrc_soprod__r.rstk__soprod_div__c ) || (   TEXT(rstk__soconrc_salesacct__r.rstk__syacc_gltype_pl__c)  &lt;&gt; &apos;Revenue&apos;)</errorConditionFormula>
        <errorMessage>Mfg Account is required and should belong to Recurring Charge Product Division.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Number_of_Periods_to_Bill</fullName>
