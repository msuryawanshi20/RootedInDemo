    <validationRules>
        <fullName>rstk__CapacityDay_Labor_Range</fullName>
        <active>true</active>
        <description>Capacity/Day, Labor Range</description>
        <errorConditionFormula>rstk__rtwctr_labhrsdaysconv__c &lt; 0 || rstk__rtwctr_labhrsdaysconv__c &gt; 24</errorConditionFormula>
        <errorMessage>Length of Production Day - Labor Hrs must be between 0 and 24</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__CapacityDay_Labor_nor_HrsDay_Labor</fullName>
        <active>true</active>
        <description>Neither &apos;Length of Production Day - Labor Hrs&apos; nor &apos;Total Capacity Labor Hrs/Day&apos; can be zero unless both are zero</description>
        <errorConditionFormula>(rstk__rtwctr_hrsperdaylab__c = 0 &amp;&amp; rstk__rtwctr_labhrsdaysconv__c != 0) || (rstk__rtwctr_hrsperdaylab__c != 0 &amp;&amp; rstk__rtwctr_labhrsdaysconv__c = 0)</errorConditionFormula>
        <errorMessage>Neither &apos;Length of Production Day - Labor Hrs&apos; nor &apos;Total Capacity Labor Hrs/Day&apos; can be zero unless both are zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__CapacityDay_Machine_Range</fullName>
        <active>true</active>
        <description>Capacity/Day, Machine Range</description>
        <errorConditionFormula>rstk__rtwctr_machhrsdaysconv__c &lt; 0 || rstk__rtwctr_machhrsdaysconv__c &gt; 24</errorConditionFormula>
        <errorMessage>Length of Production Day - Machine Hrs must be between 0 and 24</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Description</fullName>
        <active>true</active>
        <description>Description is required .</description>
        <errorConditionFormula>ISBLANK(rstk__rtwctr_descr__c)</errorConditionFormula>
        <errorMessage>Description is required .</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Division</fullName>
        <active>true</active>
        <description>Division is required and not modifiable.</description>
        <errorConditionFormula>ISBLANK(rstk__rtwctr_div__c) || ( NOT( ISNEW() ) &amp;&amp;  ISCHANGED(rstk__rtwctr_div__c)  )</errorConditionFormula>
        <errorMessage>Division is required and not modifiable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__HrsDay_Labor_Range</fullName>
