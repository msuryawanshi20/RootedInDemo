    <validationRules>
        <fullName>rstk__Clause_Title_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(rstk__somsac_title__c)</errorConditionFormula>
        <errorMessage>Clause Title is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Clause_content_required</fullName>
        <active>true</active>
        <errorConditionFormula>LEN(rstk__somsac_clause__c) == 0</errorConditionFormula>
        <errorMessage>Clause Content cannot be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Division_Required</fullName>
        <active>true</active>
        <description>Division is required</description>
        <errorConditionFormula>ISBLANK(rstk__somsac_div__c)</errorConditionFormula>
        <errorMessage>Sales Division is Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__MSA_Should_Belong_to_Division</fullName>
        <active>true</active>
        <description>Master Service Agreement should belong to the same division</description>
        <errorConditionFormula>rstk__somsac_somsa__r.rstk__somsa_div__c != rstk__somsac_div__c</errorConditionFormula>
        <errorMessage>Master Service Agreement should belong to the same division</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__MSA_required_in_new_mode</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW(), ISBLANK(rstk__somsac_somsa__c))</errorConditionFormula>
        <errorMessage>Master Service Agreement is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__MSA_seq_required_and_greater_than_0</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISBLANK(rstk__somsac_seq__c),rstk__somsac_seq__c &lt;= 0)</errorConditionFormula>
        <errorMessage>MSA Sequence is required and must be greater than 0</errorMessage>
    </validationRules>
