    <validationRules>
        <fullName>rstk__Validate_Container_Class_Div</fullName>
        <active>true</active>
        <description>Division must match Container Class Division</description>
        <errorConditionFormula>rstk__iccontainer_icconclass__r.rstk__icconclass_div__c !=  rstk__iccontainer_div__c</errorConditionFormula>
        <errorMessage>Division must match Container Class Division</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Item_Required</fullName>
        <active>true</active>
        <description>&quot;Container is Item Specific&quot; must be set if Parent Container is Item Specific</description>
        <errorConditionFormula>rstk__iccontainer_itemreqd__c = false &amp;&amp; rstk__iccontainer_iccontainer__c != null &amp;&amp; rstk__iccontainer_iccontainer__r.rstk__iccontainer_itemreqd__c = true</errorConditionFormula>
        <errorMessage>&quot;Container is Item Specific&quot; must be set if Parent Container is Item Specific</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Locid_Required</fullName>
        <active>true</active>
        <description>&quot;Container is Location ID Specific&quot; must be set if Parent Container is Location ID Specific</description>
        <errorConditionFormula>rstk__iccontainer_locidreqd__c = false &amp;&amp; rstk__iccontainer_iccontainer__c != null &amp;&amp; rstk__iccontainer_iccontainer__r.rstk__iccontainer_locidreqd__c = true</errorConditionFormula>
        <errorMessage>&quot;Container is Location ID Specific&quot; must be set if Parent Container is Location ID Specific</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Locid_Sitereqd</fullName>
        <active>true</active>
        <description>&quot;Container is Location ID Specific&quot; cannot be set unless Container is Site Specific</description>
        <errorConditionFormula>rstk__iccontainer_locidreqd__c = true &amp;&amp; rstk__iccontainer_sitereqd__c = false</errorConditionFormula>
        <errorMessage>&quot;Container is Location ID Specific&quot; cannot be set unless Container is Site Specific</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Locnum_Required</fullName>
        <active>true</active>
        <description>&quot;Container is Location Number Specific&quot; must be set if Parent Container is Location Number Specific</description>
        <errorConditionFormula>rstk__iccontainer_locnumreqd__c = false &amp;&amp; rstk__iccontainer_iccontainer__c != null &amp;&amp; rstk__iccontainer_iccontainer__r.rstk__iccontainer_locnumreqd__c = true</errorConditionFormula>
        <errorMessage>&quot;Container is Location Number Specific&quot; must be set if Parent Container is Location Number Specific</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Max_Weight</fullName>
        <active>true</active>
        <description>Max Weight cannot be less than zero</description>
        <errorConditionFormula>rstk__iccontainer_maxweight__c &lt; 0</errorConditionFormula>
        <errorMessage>Max Weight cannot be less than zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_PJPROJ</fullName>
        <active>true</active>
        <description>Project cannot be specified unless &quot;Container is Project Specific&quot; is set</description>
        <errorConditionFormula>rstk__iccontainer_pjproj__c = null &amp;&amp;  rstk__iccontainer_projreqd__c = true</errorConditionFormula>
        <errorMessage>Project cannot be specified unless &quot;Container is Project Specific&quot; is set</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Parent_Container_Div</fullName>
        <active>true</active>
        <description>Division must match Parent Container Division</description>
        <errorConditionFormula>rstk__iccontainer_iccontainer__c != null &amp;&amp; rstk__iccontainer_iccontainer__r.rstk__iccontainer_div__c !=  rstk__iccontainer_div__c</errorConditionFormula>
        <errorMessage>Division must match Parent Container Division</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Proj_Required</fullName>
