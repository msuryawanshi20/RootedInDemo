    <validationRules>
        <fullName>rstk__Covered_Serialized_Device</fullName>
        <active>true</active>
        <description>Covered Serialized Device is required .</description>
        <errorConditionFormula>ISBLANK(rstk__socondev_sowarr__c) || (ISNEW() &amp;&amp; rstk__socondev_socontract__c &lt;&gt;  rstk__socondev_sowarr__r.rstk__sowarr_socontract__c)</errorConditionFormula>
        <errorMessage>Covered Serialized Device is required .</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Current_Coverage_End_Date</fullName>
        <active>true</active>
        <description>Current Coverage End Date is required if Device is Active and must be greater than Current Coverage Start Date .</description>
        <errorConditionFormula>(rstk__socondev_active__c &amp;&amp; ISBLANK( rstk__socondev_enddate__c )) || ( rstk__socondev_enddate__c &lt;  rstk__socondev_startdate__c )</errorConditionFormula>
        <errorMessage>Current Coverage End Date is required if Device is Active and must be greater than or equal to Current Coverage Start Date .</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Current_Coverage_Start_Date</fullName>
        <active>true</active>
        <description>Current Coverage Start Date is required if Device is Active.</description>
        <errorConditionFormula>rstk__socondev_active__c &amp;&amp;  ISBLANK( rstk__socondev_startdate__c )</errorConditionFormula>
        <errorMessage>Current Coverage Start Date is required if Device is Active.</errorMessage>
    </validationRules>
