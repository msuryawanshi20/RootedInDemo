    <validationRules>
        <fullName>rstk__ProjectChargeCodeProjectValidation</fullName>
        <active>true</active>
        <description>Project Charge Code (Project) must match Project</description>
        <errorConditionFormula>rstk__poloader_pjprojwbs__r.rstk__pjprojwbs_proj__c != null &amp;&amp; 
rstk__poloader_proj__c !=  rstk__poloader_pjprojwbs__r.rstk__pjprojwbs_proj__c</errorConditionFormula>
        <errorMessage>Project Charge Code (Project) must match Project</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__ProjectChargeCodeRequiredValidation</fullName>
        <active>true</active>
        <description>Project Charge Code is required</description>
        <errorConditionFormula>OR(ISPICKVAL( rstk__poloader_mode__c , &apos;Add Line&apos;) , ISPICKVAL( rstk__poloader_mode__c , &apos;Add Both&apos;)) &amp;&amp;  rstk__poloader_proj__r.rstk__pjproj_wbsact__c = true &amp;&amp; 
 ISNULL( rstk__poloader_pjprojwbs__c )</errorConditionFormula>
        <errorMessage>Project Charge Code is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Purchase_Agreement_Validation</fullName>
        <active>true</active>
        <description>Purchase Agreement has expired</description>
        <errorConditionFormula>rstk__poloader_poagh__r.rstk__poagh_lastdate__c &lt; TODAY()</errorConditionFormula>
        <errorMessage>Purchase Agreement has expired</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Receipt_Site_Item_Type_Validation</fullName>
        <active>true</active>
        <description>Receipt Site cannot be set for Service and Indirect Material PO Item Types</description>
        <errorConditionFormula>NOT(ISBLANK(rstk__poloader_receiptsite__c)) &amp;&amp;   (ISPICKVAL(rstk__poloader_poline__r.rstk__poline_potype__c, &apos;Service&apos;) || ISPICKVAL(rstk__poloader_poline__r.rstk__poline_potype__c, &apos;	Indirect Material&apos;))</errorConditionFormula>
        <errorMessage>Receipt Site cannot be set for Service and Indirect Material PO Item Types</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Add_Line_Order_Status</fullName>
        <active>true</active>
        <description>Cannot add line to a closed Purchase Order</description>
        <errorConditionFormula>(ISNEW() || (ISCHANGED(rstk__poloader_process__c) &amp;&amp;  ISPICKVAL(rstk__poloader_process__c, &apos;Ready&apos;))) &amp;&amp; ISPICKVAL(rstk__poloader_mode__c, &apos;Add Line&apos;) &amp;&amp;   ISPICKVAL(rstk__poloader_pohdr__r.rstk__pohdr_ordsts__c, &apos;9-Closed&apos;)</errorConditionFormula>
        <errorMessage>Cannot add line to a closed Purchase Order</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__WorkOrderDivisionValidation</fullName>
        <active>true</active>
        <description>Work Order Division must match PO Line Inventory Division</description>
        <errorConditionFormula>rstk__poloader_poline__c != null &amp;&amp;  rstk__poloader_wocst__c != null &amp;&amp;  rstk__poloader_poline__r.rstk__poline_invdiv__c !=  rstk__poloader_wocst__r.rstk__wocst_div__c</errorConditionFormula>
        <errorMessage>Work Order Division must match PO Line Inventory Division</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__WorkOrderItemValidation</fullName>
        <active>true</active>
        <description>Work Order Item must match PO Line Item</description>
        <errorConditionFormula>rstk__poloader_poline__c != null &amp;&amp;  rstk__poloader_wocst__c != null &amp;&amp;  rstk__poloader_poline__r.rstk__poline_item__r.rstk__poitem_item__c !=  rstk__poloader_wocst__r.rstk__wocst_item__c</errorConditionFormula>
        <errorMessage>Work Order Item must match PO Line Item</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__WorkOrderOpSubflagValidation</fullName>
        <active>true</active>
        <description>Work Order Operation must an Outside Operation</description>
        <errorConditionFormula>rstk__poloader_woordop__c != null &amp;&amp;  rstk__poloader_woordop__r.rstk__woordop_subflag__c = false</errorConditionFormula>
        <errorMessage>Work Order Operation must an Outside Operation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__WorkOrderProjectChargeCodeValidation</fullName>
        <active>true</active>
        <description>Work Order Project Charge Code must match specified Project Charge Code</description>
        <errorConditionFormula>rstk__poloader_pjprojwbs__c != null &amp;&amp;  rstk__poloader_wocst__c != null &amp;&amp;  rstk__poloader_pjprojwbs__c !=  rstk__poloader_wocst__r.rstk__wocst_pjprojwbs__c</errorConditionFormula>
        <errorMessage>Work Order Project Charge Code must match specified Project Charge Code</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__WorkOrderProjectValidation</fullName>
        <active>true</active>
        <description>Work Order Project must match specified Project</description>
        <errorConditionFormula>rstk__poloader_proj__c != null &amp;&amp;  rstk__poloader_wocst__c != null &amp;&amp;  rstk__poloader_proj__c !=  rstk__poloader_wocst__r.rstk__wocst_proj__c</errorConditionFormula>
        <errorMessage>Work Order Project must match specified Project</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__WorkOrderSiteValidation</fullName>
        <active>true</active>
        <description>Work Order Target Site must match PO Line Receipt Site</description>
        <errorConditionFormula>rstk__poloader_poline__c != null &amp;&amp;  rstk__poloader_wocst__c != null &amp;&amp;  rstk__poloader_poline__r.rstk__poline_receiptsite__c !=  rstk__poloader_wocst__r.rstk__wocst_targetsite__c</errorConditionFormula>
        <errorMessage>Work Order Target Site must match PO Line Receipt Site</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__WorkOrderStatusValidation</fullName>
        <active>true</active>
        <description>Work Order Status must be Less than 8-Receipts completed</description>
        <errorConditionFormula>ISNEW() &amp;&amp; rstk__poloader_wocst__c != null &amp;&amp; (rstk__poloader_wocst__r.rstk__wocst_ordsts__c = &apos;8&apos; || rstk__poloader_wocst__r.rstk__wocst_ordsts__c = &apos;9&apos;)</errorConditionFormula>
        <errorMessage>Work Order Status must be Less than 8-Receipts completed</errorMessage>
    </validationRules>
