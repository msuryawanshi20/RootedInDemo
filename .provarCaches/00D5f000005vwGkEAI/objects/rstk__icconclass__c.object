    <validationRules>
        <fullName>rstk__Validate_Container_Type_Company</fullName>
        <active>true</active>
        <description>Container Type Company must match Divisions Company</description>
        <errorConditionFormula>rstk__icconclass_socontype__c != null &amp;&amp; rstk__icconclass_socontype__r.rstk__socontype_div__r.rstk__sydiv_cmpno__c !=  rstk__icconclass_div__r.rstk__sydiv_cmpno__c</errorConditionFormula>
        <errorMessage>Container Type Company must match Divisions Company</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Container_is_Location_ID</fullName>
        <active>true</active>
        <description>Container is Location ID Specific cannot be set unless Container is Site Specific is set</description>
        <errorConditionFormula>rstk__icconclass_locidreqd__c = true &amp;&amp; rstk__icconclass_sitereqd__c = false</errorConditionFormula>
        <errorMessage>Container is Location ID Specific cannot be set unless Container is Site Specific is set</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Container_is_Location_Number</fullName>
        <active>true</active>
        <description>Container is Location Number Specific cannot be set unless Container is Location ID Specific is set</description>
        <errorConditionFormula>rstk__icconclass_locnumreqd__c = true &amp;&amp;  rstk__icconclass_locidreqd__c = false</errorConditionFormula>
        <errorMessage>Container is Location Number Specific cannot be set unless Container is Location ID Specific is set</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Max_Net_Weight</fullName>
        <active>true</active>
        <description>Max Net Weight cannot be less than 0</description>
        <errorConditionFormula>rstk__icconclass_maxnetweight__c &lt; 0</errorConditionFormula>
        <errorMessage>Max Net Weight cannot be less than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Tare_Weight</fullName>
        <active>true</active>
        <description>Container Tare Weight cannot be less than 0</description>
        <errorConditionFormula>rstk__icconclass_tareweight__c &lt; 0</errorConditionFormula>
        <errorMessage>Container Tare Weight cannot be less than 0</errorMessage>
    </validationRules>
