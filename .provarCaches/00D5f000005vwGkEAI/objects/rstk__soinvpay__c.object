IF(rstk__soinvpay_cardtype__c == 5, &apos;UKMaestro&apos;,
IF(rstk__soinvpay_cardtype__c == 6, &apos;CarteAurore&apos;,
IF(rstk__soinvpay_cardtype__c == 7, &apos;CarteBleue&apos;,
IF(rstk__soinvpay_cardtype__c == 8, &apos;Cofinoga&apos;,
IF(rstk__soinvpay_cardtype__c == 9, &apos;4etoiles&apos;,
IF(rstk__soinvpay_cardtype__c == 10, &apos;CarteAura&apos;,
IF(rstk__soinvpay_cardtype__c == 11, &apos;TarjetaAurora&apos;,
IF(rstk__soinvpay_cardtype__c == 12, &apos;JCB&apos;,
IF(rstk__soinvpay_cardtype__c == 13, &apos;Maestro&apos;, &apos;&apos;))))))))))))))</formula>
