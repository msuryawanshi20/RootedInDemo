    <validationRules>
        <fullName>rstkf__fullaccessusercheck_addingnewfinuser</fullName>
        <active>true</active>
        <description>Financial Users must be Full Access Users</description>
        <errorConditionFormula>ISNEW() &amp;&amp; rstkf__glusr_syusr__r.rstk__syusr_licreq__c = false</errorConditionFormula>
        <errorMessage>Financial Users must be Full Access Users</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstkf__fullaccessusercheck_editexistingfinuser</fullName>
        <active>true</active>
        <description>Financial Users must be Full Access Users</description>
        <errorConditionFormula>AND(ISCHANGED(rstkf__glusr_inactive__c), rstkf__glusr_inactive__c = false) &amp;&amp; rstkf__glusr_syusr__r.rstk__syusr_licreq__c = false</errorConditionFormula>
        <errorMessage>Financial Users must be Full Access Users</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstkf__manufacturingusrsisreqd</fullName>
        <active>true</active>
        <description>Manufacturing Users is required and cannot be modifiable</description>
        <errorConditionFormula>ISBLANK( rstkf__glusr_syusr__c ) ||  ISNULL( rstkf__glusr_syusr__c ) 
|| ( !ISNEW() &amp;&amp;  ISCHANGED( rstkf__glusr_syusr__c) )</errorConditionFormula>
        <errorMessage>Manufacturing Users is required and cannot be modifiable</errorMessage>
    </validationRules>
