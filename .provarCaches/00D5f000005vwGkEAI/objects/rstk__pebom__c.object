    <validationRules>
        <fullName>rstk__Check_Add_Status_Values</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR(ISPICKVAL( rstk__pebom_addsts_pl__c , &apos;Pending&apos;), ISPICKVAL( rstk__pebom_addsts_pl__c , &apos;Released&apos;), ISPICKVAL( rstk__pebom_addsts_pl__c , &apos;Implemented&apos;)))</errorConditionFormula>
        <errorDisplayField>rstk__pebom_addsts_pl__c</errorDisplayField>
        <errorMessage>Unrecognized Add Status (valid values: Pending, Released or Implemented).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Check_Del_Status_Values</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR(ISPICKVAL( rstk__pebom_delsts_pl__c , &apos;Pending&apos;), ISPICKVAL( rstk__pebom_delsts_pl__c , &apos;Released&apos;), ISPICKVAL( rstk__pebom_delsts_pl__c , &apos;Implemented&apos;),ISPICKVAL( rstk__pebom_delsts_pl__c , &apos;&apos;)))</errorConditionFormula>
        <errorDisplayField>rstk__pebom_delsts_pl__c</errorDisplayField>
        <errorMessage>Unrecognized Delete Status (valid values: Pending, Released or Implemented).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Check_Edit_AddRev_Change</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(PRIORVALUE(rstk__pebom_addrev__c) = rstk__pebom_addrev__c)</errorConditionFormula>
        <errorDisplayField>rstk__pebom_compitem__c</errorDisplayField>
        <errorMessage>Add Revision cannot be changed (delete and re-add record).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Check_Edit_CompItem_Change</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(PRIORVALUE(rstk__pebom_compitem__c) = rstk__pebom_compitem__c)</errorConditionFormula>
        <errorDisplayField>rstk__pebom_compitem__c</errorDisplayField>
        <errorMessage>Component Item cannot be changed (delete and re-add record).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Check_Edit_Item_Change</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(PRIORVALUE(rstk__pebom_item__c) = rstk__pebom_item__c)</errorConditionFormula>
        <errorDisplayField>rstk__pebom_item__c</errorDisplayField>
        <errorMessage>Parent Item cannot be changed (delete and re-add record).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Check_Phantom_Override_Values</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR(ISPICKVAL( rstk__pebom_phantomovr_pl__c , &apos;Use as Standard Component&apos;), ISPICKVAL( rstk__pebom_phantomovr_pl__c , &apos;&apos;)))</errorConditionFormula>
        <errorDisplayField>rstk__pebom_phantomovr_pl__c</errorDisplayField>
        <errorMessage>Unrecognized Phantom Override (valid values: blank or &apos;Use as Standard Component&apos;).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Check_Required_Component</fullName>
        <active>true</active>
        <errorConditionFormula>rstk__pebom_compitem__c == null</errorConditionFormula>
        <errorDisplayField>rstk__pebom_compitem__c</errorDisplayField>
        <errorMessage>Component Item is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Check_Required_Parent_Item</fullName>
        <active>true</active>
        <errorConditionFormula>rstk__pebom_item__c == null</errorConditionFormula>
        <errorDisplayField>rstk__pebom_item__c</errorDisplayField>
        <errorMessage>Parent item is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Different_Parent_vs_Component_Items</fullName>
        <active>true</active>
        <errorConditionFormula>rstk__pebom_item__c == rstk__pebom_compitem__c</errorConditionFormula>
        <errorMessage>Parent Item and Component Item cannot be the same.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Inventory_Revision_Master</fullName>
        <active>true</active>
        <description>Inventory Revision Master Items cannot have a Bill of Material</description>
        <errorConditionFormula>rstk__pebom_item__r.rstk__peitem_invrevmaster__c == true</errorConditionFormula>
        <errorMessage>Inventory Revision Master Items cannot have a Bill of Material</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Inventory_Revision_Master_Component</fullName>
        <active>true</active>
        <description>Inventory Revision Master Items cannot be used on a Bill of Material</description>
        <errorConditionFormula>rstk__pebom_compitem__r.rstk__peitem_invrevmaster__c == true</errorConditionFormula>
        <errorMessage>Inventory Revision Master Items cannot be used on a Bill of Material</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Non_Negative_Qty_Per</fullName>
        <active>true</active>
        <errorConditionFormula>rstk__pebom_qtyper__c &lt; 0</errorConditionFormula>
        <errorDisplayField>rstk__pebom_qtyper__c</errorDisplayField>
        <errorMessage>Quantity Per must be greater than or equal to zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Non_Rev_Ctrl_Check_Add_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT( rstk__pebom_div__r.rstk__sydiv_revcntlact__c),ISBLANK(rstk__pebom_adddate__c))</errorConditionFormula>
        <errorDisplayField>rstk__pebom_adddate__c</errorDisplayField>
        <errorMessage>Add Date is required (Division is not under Revision Control).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Non_Rev_Ctrl_Check_Add_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT( rstk__pebom_div__r.rstk__sydiv_revcntlact__c),ISPICKVAL(rstk__pebom_addsts_pl__c,&quot;&quot;))</errorConditionFormula>
        <errorDisplayField>rstk__pebom_addsts_pl__c</errorDisplayField>
        <errorMessage>Add Status is required (Division is not under Revision Control).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Non_Rev_Ctrl_Check_Del_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT( rstk__pebom_div__r.rstk__sydiv_revcntlact__c),AND(NOT(ISBLANK(rstk__pebom_deldate__c)),ISPICKVAL(rstk__pebom_delsts_pl__c,&quot;&quot;)))</errorConditionFormula>
        <errorDisplayField>rstk__pebom_delsts_pl__c</errorDisplayField>
        <errorMessage>Delete Status is required if Delete Date is specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Non_Rev_Ctrl_Check_Del_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT( rstk__pebom_div__r.rstk__sydiv_revcntlact__c),AND(ISBLANK(rstk__pebom_deldate__c),NOT(ISPICKVAL(rstk__pebom_delsts_pl__c,&quot;&quot;))))</errorConditionFormula>
        <errorDisplayField>rstk__pebom_deldate__c</errorDisplayField>
        <errorMessage>Delete Date is required if Delete Status is specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Override_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISPICKVAL(rstk__pebom_compitem__r.rstk__peitem_type_pl__c,&apos;Phantom&apos;)), ISPICKVAL( rstk__pebom_phantomovr_pl__c,&apos;Use as Standard Component&apos;))</errorConditionFormula>
        <errorDisplayField>rstk__pebom_phantomovr_pl__c</errorDisplayField>
        <errorMessage>Component must be type Phantom to have Phantom Overrider Indicator of &apos;Use as Standard Component&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Phantom_Override_Fixed_Quantity</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(rstk__pebom_compitem__r.rstk__peitem_type_pl__c  , &apos;Phantom&apos;) &amp;&amp;  rstk__pebom_fixqtyflag__c == true</errorConditionFormula>
        <errorDisplayField>rstk__pebom_phantomovr_pl__c</errorDisplayField>
        <errorMessage>Fixed Quantity cannot be selected when Component Item type is &apos;Phantom&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Rev_Check</fullName>
        <active>true</active>
        <errorConditionFormula>rstk__pebom_div__r.rstk__sydiv_revcntlact__c &amp;&amp; rstk__pebom_addrev__c==null</errorConditionFormula>
        <errorDisplayField>rstk__pebom_addrev__c</errorDisplayField>
        <errorMessage>Add Revision is required (Revision control is active for Division).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Scrap_Factor_Range</fullName>
        <active>true</active>
        <errorConditionFormula>IF(!ISNULL(rstk__pebom_scrappct__c),OR(rstk__pebom_scrappct__c&lt;0, rstk__pebom_scrappct__c&gt;=1),false)</errorConditionFormula>
        <errorDisplayField>rstk__pebom_scrappct__c</errorDisplayField>
        <errorMessage>Please enter a Scrap Factor greater than or equal to 0 and less than 1.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Setup_Quantity_Fixed_Quantity</fullName>
        <active>true</active>
        <description>Setup Quantity cannot be greater than Qty Per for Fixed Quantity</description>
        <errorConditionFormula>rstk__pebom_fixqtyflag__c == true &amp;&amp;  rstk__pebom_setupqty__c &gt;  rstk__pebom_qtyper__c</errorConditionFormula>
        <errorMessage>Setup Quantity cannot be greater than Qty Per for Fixed Quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Setup_Quantity_Qty_Per</fullName>
        <active>true</active>
        <description>Setup Quantity must be zero if Qty Per is zero.</description>
        <errorConditionFormula>rstk__pebom_qtyper__c == 0 &amp;&amp;  rstk__pebom_setupqty__c != 0</errorConditionFormula>
        <errorMessage>Setup Quantity must be zero if Qty Per is zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_AddEvent_Compitem</fullName>
        <active>true</active>
        <description>Validate Add PEBOM from Compitem</description>
        <errorConditionFormula>ISPICKVAL( rstk__pebom_compitem__r.rstk__peitem_itemtype__c , 
&apos;Labor Only&apos;)</errorConditionFormula>
        <errorMessage>Labor Only Items may not be added as BOM Components</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_AddEvent_Item</fullName>
        <active>true</active>
        <description>Check Add PEBOM from Item</description>
        <errorConditionFormula>ISPICKVAL(rstk__pebom_item__r.rstk__peitem_itemtype__c,
&apos;Labor Only&apos;)</errorConditionFormula>
        <errorMessage>Labor Only Items may not have BOM structures</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Validate_Item_Revision</fullName>
        <active>true</active>
        <description>Add Revision&apos;s Item must match Parent Item</description>
        <errorConditionFormula>rstk__pebom_addrev__r.rstk__peitemrev_item__c != null &amp;&amp; rstk__pebom_item__c != null &amp;&amp; rstk__pebom_addrev__r.rstk__peitemrev_item__c != rstk__pebom_item__c</errorConditionFormula>
        <errorMessage>Add Revision&apos;s Item must match Parent Item</errorMessage>
    </validationRules>
