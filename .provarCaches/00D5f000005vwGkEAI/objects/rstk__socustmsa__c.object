    </validationRules>
    <validationRules>
        <fullName>rstk__Effective_Date</fullName>
        <active>true</active>
        <description>Last Effective Date cannot be less than First Effective Date.</description>
        <errorConditionFormula>!ISBLANK(rstk__socustmsa_startdate__c) &amp;&amp; !ISBLANK(rstk__socustmsa_enddate__c) &amp;&amp; rstk__socustmsa_enddate__c &lt;  rstk__socustmsa_startdate__c</errorConditionFormula>
        <errorMessage>Last Effective Date cannot be less than First Effective Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Template_MSA_should_belong_to_same_div</fullName>
        <active>true</active>
        <description>Template Division should belong to the same division</description>
        <errorConditionFormula>rstk__socustmsa_somsa__r.rstk__somsa_div__c != rstk__socustmsa_div__c</errorConditionFormula>
        <errorMessage>Template Division should belong to the same division</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Template_MSA_validation</fullName>
        <active>true</active>
        <errorConditionFormula>OR(rstk__socustmsa_somsa__r.rstk__somsa_div__c != rstk__socustmsa_div__c, rstk__socustmsa_somsa__r.rstk__somsa_active__c=false)</errorConditionFormula>
        <errorMessage>MSA template division should match Customer MSA division and the MSA should be active.</errorMessage>
    </validationRules>
