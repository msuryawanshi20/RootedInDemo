    <validationRules>
        <fullName>rstk__Activation_Grace_Period</fullName>
        <active>true</active>
        <description>Activation Grace Period (Days)  cannot be less than zero.</description>
        <errorConditionFormula>!ISBLANK( rstk__socontract_actgracedays__c ) &amp;&amp; rstk__socontract_actgracedays__c &lt; 0</errorConditionFormula>
        <errorMessage>Activation Grace Period (Days)  cannot be less than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Cannot_change_monthly_or_total_minimum</fullName>
        <active>true</active>
        <description>Cannot change monthly or total minimum values when Minimum Committed Volume is enabled</description>
        <errorConditionFormula>AND(NOT(rstk__socontract_mincomvol__c),  OR(ISCHANGED(rstk__socontract_monmincopycol__c), ISCHANGED(rstk__socontract_monmincopybw__c), ISCHANGED(rstk__socontract_monmincopyscan__c), ISCHANGED(rstk__socontract_totmincopycol__c), ISCHANGED(rstk__socontract_totmincopybw__c), ISCHANGED(rstk__socontract_totmincopyscan__c)))</errorConditionFormula>
        <errorMessage>Cannot change monthly or total minimum values when Minimum Committed Volume is enabled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Cant_change_contract_id</fullName>
        <active>true</active>
        <description>Can&apos;t change contract id</description>
        <errorConditionFormula>ISCHANGED(rstk__socontract_contractid__c)</errorConditionFormula>
        <errorMessage>The Contract ID cannot be changed after the Contract is created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Cant_change_customer</fullName>
        <active>true</active>
        <description>Customer is required and  cannot be changed after the Contract is created.</description>
        <errorConditionFormula>ISBLANK(rstk__socontract_socust__c)</errorConditionFormula>
        <errorMessage>Customer is required and  cannot be changed after the Contract is created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Cant_change_division</fullName>
        <active>true</active>
        <description>Sales Division required and  cannot be changed after the Contract is created.</description>
        <errorConditionFormula>ISBLANK(rstk__socontract_div__c) || (! ISNEW() &amp;&amp; ISCHANGED(rstk__socontract_div__c))</errorConditionFormula>
        <errorMessage>Sales Division required and  cannot be changed after the Contract is created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Contract_Term</fullName>
        <active>true</active>
        <description>Contract Term is Required and must be greater than zero.</description>
        <errorConditionFormula>ISBLANK( rstk__socontract_term__c ) || rstk__socontract_term__c  &lt;= 0</errorConditionFormula>
        <errorMessage>Contract Term is Required and must be greater than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Contract_Terms_Template</fullName>
        <active>false</active>
        <description>Contract Terms Template is not modifiable as Contract not in Draft status,also it should be active and should belong to contract Division.</description>
        <errorConditionFormula>(!ISNEW() &amp;&amp; ISCHANGED(rstk__socontract_soctt__c ) &amp;&amp; TEXT(rstk__socontract_constatus__c ) &lt;&gt; &apos;Draft&apos;) || (!ISBLANK(rstk__socontract_soctt__c) &amp;&amp; ( rstk__socontract_soctt__r.rstk__soctt_div__c  &lt;&gt;  rstk__socontract_div__c  ||  !rstk__socontract_soctt__r.rstk__soctt_active__c ) )</errorConditionFormula>
        <errorMessage>Contract Terms Template is not modifiable as Contract not in Draft status,also it should be active and should belong to contract Division.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Customer_for_Recurring_Billings</fullName>
        <active>true</active>
        <description>Customer for Recurring Billing is Required and not modifiable as Contract not in Draft status.</description>
        <errorConditionFormula>ISBLANK(rstk__socontract_servicecust__c)</errorConditionFormula>
        <errorMessage>Customer for Recurring Billing is Required and not modifiable as Contract not in Draft status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__First_Full_RB_Period_Start_Dt_Validation</fullName>
        <active>true</active>
        <description>First Full RB Period Start Date must be greater than Recurring Billing Start Date</description>
        <errorConditionFormula>NOT(ISBLANK(rstk__socontract_rcbfirstfull__c)) &amp;&amp; (rstk__socontract_rcbfirstfull__c &lt;= rstk__socontract_rcbstart__c)</errorConditionFormula>
        <errorMessage>First Full RB Period Start Date must be greater than Recurring Billing Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Hardware_Installation_Customer</fullName>
        <active>true</active>
        <description>Hardware Installation Customer is Required and not modifiable as Contract not in Draft status.</description>
        <errorConditionFormula>ISBLANK(rstk__socontract_installcust__c)</errorConditionFormula>
        <errorMessage>Hardware Installation Customer is Required and not modifiable as Contract not in Draft status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Installation_Customer_Address</fullName>
        <active>true</active>
        <description>Installation Customer Address is Required, should be Valid Address for Installation and also should belong to Hardware Installation Customer.</description>
        <errorConditionFormula>ISBLANK(rstk__socontract_icustinstaddr__c) || rstk__socontract_icustinstaddr__r.rstk__socaddr_custno__c &lt;&gt; rstk__socontract_installcust__c  || (!rstk__socontract_icustinstaddr__r.rstk__socaddr_defaultinstall__c &amp;&amp; !rstk__socontract_icustinstaddr__r.rstk__socaddr_useasinstall__c)</errorConditionFormula>
        <errorMessage>Installation Customer Address is Required, should be Valid Address for Installation and also should belong to Hardware Installation Customer.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Master_Services_Agreement</fullName>
        <active>false</active>
        <description>Master Services Agreement is not modifiable as Contract not in Draft status,also it should be active and belong to Contract Customer.</description>
        <errorConditionFormula>(!ISNEW() &amp;&amp; ISCHANGED(rstk__socontract_socustmsa__c ) &amp;&amp; TEXT(rstk__socontract_constatus__c ) &lt;&gt; &apos;Draft&apos;) || (!ISBLANK(rstk__socontract_socustmsa__c) &amp;&amp; ( rstk__socontract_socustmsa__r.rstk__socustmsa_socust__c &lt;&gt; rstk__socontract_socust__c ||  !rstk__socontract_socustmsa__r.rstk__socustmsa_active__c ))</errorConditionFormula>
        <errorMessage>Master Services Agreement is not modifiable as Contract not in Draft status,also it should be active and belong to Contract Customer.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Opportunity_Cannot_be_modified</fullName>
        <active>true</active>
        <description>Opportunity Cannot be modified</description>
        <errorConditionFormula>ISCHANGED(rstk__socontract_opportunity__c)</errorConditionFormula>
        <errorDisplayField>rstk__socontract_opportunity__c</errorDisplayField>
        <errorMessage>Opportunity Cannot be modified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Pricing_Customer</fullName>
        <active>true</active>
        <description>Pricing Customer is Required and not modifiable as Contract not in Draft status.</description>
        <errorConditionFormula>ISBLANK(rstk__socontract_pricecust__c)</errorConditionFormula>
        <errorMessage>Pricing Customer is Required and not modifiable as Contract not in Draft status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Recurring_Billing_Frequency</fullName>
        <active>true</active>
        <description>Recurring Billing Frequency is not modifiable as Last Recurring Bill Thru Date  in already set.</description>
        <errorConditionFormula>!ISNEW() &amp;&amp; ISCHANGED(rstk__socontract_rcbfreq__c) &amp;&amp; !ISNULL(rstk__socontract_rcblastthru__c )</errorConditionFormula>
        <errorMessage>Recurring Billing Frequency is not modifiable as Last Recurring Bill Thru Date  in already set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Recurring_Billing_Start_Date</fullName>
        <active>true</active>
        <description>Recurring Billing Start Date is not modifiable as Last Recurring Bill Thru Date  in already set.</description>
        <errorConditionFormula>!ISNEW() &amp;&amp; ISCHANGED(rstk__socontract_rcbstart__c) &amp;&amp; !ISNULL(rstk__socontract_rcblastthru__c )</errorConditionFormula>
        <errorMessage>Recurring Billing Start Date is not modifiable as Last Recurring Bill Thru Date  in already set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Recurring_Billings_in_Advance</fullName>
        <active>true</active>
        <description>Recurring Billings in Advance field is not modifiable as Last Recurring Bill Thru Date  in already set.</description>
        <errorConditionFormula>!ISNEW() &amp;&amp; ISCHANGED(rstk__socontract_rcbadvance__c) &amp;&amp; !ISNULL(rstk__socontract_rcblastthru__c )</errorConditionFormula>
        <errorMessage>Recurring Billings in Advance field is not modifiable as Last Recurring Bill Thru Date  in already set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Sales_Customer</fullName>
        <active>true</active>
        <description>Sales Customer is Required and not modifiable as Contract not in Draft status.</description>
        <errorConditionFormula>ISBLANK(rstk__socontract_sohdrcust__c)</errorConditionFormula>
        <errorMessage>Sales Customer is Required and not modifiable as Contract not in Draft status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Sales_Customer_BillTo_Address</fullName>
        <active>true</active>
        <description>Sales Customer Bill-To Address is Required , should be Valid  Address for Billing and also  should belong to Sales Customer.</description>
        <errorConditionFormula>ISBLANK(rstk__socontract_btaddr__c) ||  rstk__socontract_btaddr__r.rstk__socaddr_custno__c &lt;&gt; rstk__socontract_sohdrcust__c || (!rstk__socontract_btaddr__r.rstk__socaddr_defaultbillto__c &amp;&amp; !rstk__socontract_btaddr__r.rstk__socaddr_useasbillto__c)</errorConditionFormula>
        <errorMessage>Sales Customer Bill-To Address is Required , should be Valid  Address for Billing and also  should belong to Sales Customer.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Sales_Customer_ShipTo_Address</fullName>
        <active>true</active>
        <description>Sales Customer Ship-To Address is Required, should be Valid Address for Shipping  and also should belong to Sales Customer.</description>
        <errorConditionFormula>ISBLANK(rstk__socontract_staddr__c)|| rstk__socontract_staddr__r.rstk__socaddr_custno__c &lt;&gt; rstk__socontract_sohdrcust__c || (!rstk__socontract_staddr__r.rstk__socaddr_defaultshipto__c &amp;&amp; !rstk__socontract_staddr__r.rstk__socaddr_useasshipto__c)</errorConditionFormula>
        <errorMessage>Sales Customer Ship-To Address is Required, should be Valid Address for Shipping  and also should belong to Sales Customer.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Sales_Division</fullName>
        <active>true</active>
        <description>Sales Division is required and not Modifiable.</description>
        <errorConditionFormula>ISBLANK(rstk__socontract_div__c ) || (! ISNEW() &amp;&amp; ISCHANGED(rstk__socontract_div__c) )</errorConditionFormula>
        <errorMessage>Sales Division is required and not Modifiable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Service_Customer_BillTo_Address</fullName>
        <active>true</active>
        <description>Service Customer Bill-To Address is Required, should be Valid Address for Billing and also should belong to Customer for Recurring Billings.</description>
        <errorConditionFormula>ISBLANK(rstk__socontract_scustbtaddr__c) || rstk__socontract_scustbtaddr__r.rstk__socaddr_custno__c &lt;&gt; rstk__socontract_servicecust__c || (!rstk__socontract_scustbtaddr__r.rstk__socaddr_defaultbillto__c &amp;&amp; !rstk__socontract_scustbtaddr__r.rstk__socaddr_useasbillto__c)</errorConditionFormula>
        <errorMessage>Service Customer Bill-To Address is Required, should be Valid Address for Billing and also should belong to Customer for Recurring Billings.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__socontract_taxloc_validation</fullName>
