    <validationRules>
        <fullName>rstk__Activation_Grace_Period_Days</fullName>
        <active>true</active>
        <description>Activation Grace Period (Days) cannot be blank and should not be less than zero.</description>
        <errorConditionFormula>ISBLANK(rstk__soconpp_actgracedays__c) || rstk__soconpp_actgracedays__c  &lt; 0</errorConditionFormula>
        <errorMessage>Activation Grace Period (Days) cannot be blank and should not be less than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Associated_Recurring_Charge_Product</fullName>
        <active>true</active>
        <description>Associated Recurring Charge Product is Required.</description>
        <errorConditionFormula>rstk__soconpp_soprod__r.rstk__formula_reqservice__c &amp;&amp;  ISBLANK( rstk__soconpp_rcprod__c )</errorConditionFormula>
        <errorMessage>Associated Recurring Charge Product is Required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Commitment_Quantity</fullName>
        <active>true</active>
        <description>Commitment Quantity is required , must be greater than zero also should not be less than Quantity Purchased.</description>
        <errorConditionFormula>ISBLANK(rstk__soconpp_commitqty__c) || rstk__soconpp_commitqty__c  &lt;= 0 ||  rstk__soconpp_commitqty__c  &lt; rstk__soconpp_qtysold__c</errorConditionFormula>
        <errorMessage>Error saving Contract Product for Purchase. The required Commitment Quantity cannot be less than the Quantity Purchased.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstk__Discount_Pct</fullName>
        <active>true</active>
        <description>Discount Pct should not be less than zero.</description>
