        <fullName>rstkf__Deposit_Reference_not_allow_to_update</fullName>
        <active>true</active>
        <description>Deposit Reference field cannot be updated if the Deposit is in Posted status</description>
        <errorConditionFormula>PRIORVALUE( rstkf__arcashh_depref__c ) &lt;&gt;  rstkf__arcashh_depref__c  &amp;&amp;   ISPICKVAL( rstkf__arcashh_poststatus__c , &apos;Posted&apos;)</errorConditionFormula>
        <errorMessage>Deposit Reference field cannot be updated if the Deposit is in Posted status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rstkf__batchamthome_less_than_paymentshome</fullName>
        <active>false</active>
        <description>Deposit Amount - Home cannot be less than the Sum of Customer Payments - Home Currency</description>
        <errorConditionFormula>AND( NOT(ISCHANGED(rstkf__arcashh_depdate__c)),  NOT(ISNEW()),   
ROUND(rstkf__arcashh_batchamthome__c,2) &lt; ROUND(rstkf__arcashh_paymentshome__c,2))</errorConditionFormula>
        <errorDisplayField>rstkf__altextid__c</errorDisplayField>
        <errorMessage>Deposit Amount - Home cannot be less than the Sum of Customer Payments - Home Currency</errorMessage>
    </validationRules>
