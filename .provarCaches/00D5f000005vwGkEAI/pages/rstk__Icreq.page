<c:standardHeader cntr="{!controller}" title="{!$Label.rstk__pagetitle_icreq}" customInclude="{!$Resource.rstk__icreq_cli}"  enableAutocomplete="true"/>

<script>
    var isNew = {!isNew};
    var isEdit = {!isEdit};
    var isView = {!isView};
    var isBoc = {!isBoc};
    var reqStatus = '{!o.icreq_ordsts_pl__c}';
    var dateUpdateAllowed = {!currDivision.sydiv_reqdateupdate__c};
</script>

