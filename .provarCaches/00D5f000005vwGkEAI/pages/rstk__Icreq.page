            <apex:tab label="{!$Label.rstk__pagetab_icreq_info}" name="tab_Info" id="tab_Info">
                <c:infoSection cntr="{!controller}" showHeader="false"/>
            </apex:tab>
        </apex:tabPanel>
    </apex:pageBlock>
</apex:form>

<c:footerInclude cntr="{!controller}" />
<script>
        var Requisition = "{!$Label.RequisitionNotApproved}"; 
