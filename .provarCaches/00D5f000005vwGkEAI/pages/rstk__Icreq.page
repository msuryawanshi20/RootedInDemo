            </apex:pageBlockSectionItem>
            
	        <apex:pageBlockSectionItem helpText="{!f.rstk__icreq_receiptlocid__c.inlineHelpText}">
				<apex:outputLabel value="{!f.rstk__icreq_receiptlocid__c.label}"/>
