            <apex:inputHidden id="icreq_wbs__c" value="{!o.rstk__icreq_wbs__c}"/>
            <apex:inputHidden id="icreq_effrev__c" value="{!o.rstk__icreq_effrev__c}"/>
            <apex:inputHidden id="icreq_puruom__c" value="{!o.rstk__icreq_puruom__c}"/>
