        .Processing
        {
            position: fixed;
            background: url({!$Resource.busy});
            background-repeat: no-repeat;
            background-position: center;
            width: 100%;
            height: 100%;
            z-index: 2004;
            left: 0%;
            top: 0%;
        }
    </style>
    <script>
        function verifyDate(firstDate,secondDate,fieldID){
            Visualforce.remoting.Manager.invokeAction(  
                '{!$RemoteAction.ControllerExtnCreditAppReport.verifyDate}', //NameSpace  
                firstDate,
                secondDate,   
                function(result, event){
                    if (event.status) {
                        checkResult(result,fieldID);
                    } else if (event.type === 'exception') {
                        checkException(event.message,fieldID);
                    } else {
                        alert("{!$Label.UnexpectedStatus}", event.message);
                    }
                }, {escape:false}
            );
        }
    </script>
    
