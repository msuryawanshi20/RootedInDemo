 			<apex:outputPanel rendered="{!isCostedBOM}">
 			<td>{!$Label.rstk__ohd_labor}</td>
 			</apex:outputPanel>
 			<apex:outputPanel rendered="{!isCostedBOM}">
 			<td>{!$Label.rstk__ohd_fringe}</td>
 			</apex:outputPanel>
