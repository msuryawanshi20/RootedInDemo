        </apex:actionFunction>
        <apex:actionFunction name="doAssignSerialsFromPrimary" action="{!doAssignSerialsFromPrimary}" rerender="pageMessages,op_serialnumbers" immediate="true" oncomplete="unblockUI();" status="statusProcessing">
