            <apex:param name="componentListDisplay" assignTo="{!componentListDisplay}" value="" />
            <apex:param name="operationListDisplay" assignTo="{!operationListDisplay}" value="" />
        </apex:actionFunction>
        <apex:actionFunction name="doBackscheduleOperations" action="{!doBackscheduleOperations}" rerender="pageMessages,op_Dates,op_Misc,op_Operations" immediate="true" oncomplete="unblockUI();"/>
        <apex:actionFunction name="doExtractOperations" action="{!doExtractOperations}" rerender="pageMessages,op_resultVars" immediate="true" oncomplete="if (actionError == false) {window.location.reload();} else {unblockUI();}">
