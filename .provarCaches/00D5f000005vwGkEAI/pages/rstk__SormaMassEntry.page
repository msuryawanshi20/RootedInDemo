				                   		var sysitefld = newRmaDetail.getRowObj(row, 'sorma_site__c')[0];
				                   		
				                   		if (solinefld && solinefld.getAttribute('sorma_soprod__c_fieldid') == null) {
				                   			solinefld.setAttribute('sorma_soprod__c_fieldid', soprodfld.id);
				                   		}
