function enableFixedHeaderSort(tableId) {
    var tbl = jQuery("table[id$=" + tableId + "]");
    var sortOptions = { 
            0: { sorter: false },
            1: { sorter: false },
            2: { sorter: false },
            3: { sorter: false },
            4: { sorter: false }
        };

    if (tbl) {
        tbl.vfFloatingHeaders();
        tbl.tablesorter({
            headers: sortOptions
        });
    }
}

