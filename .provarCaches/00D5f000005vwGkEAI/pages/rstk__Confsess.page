                                 	<c:largeSelectList id="confsess_staddr__c" cntr="{!controller}" options="{!shipToAdresses}"/>
                                 </apex:outputPanel>
                            </apex:pageBlockSectionItem>
                            
                            <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_comment__c.inlineHelpText}">
                                <apex:outputLabel value="{!f.rstk__confsess_comment__c.label}"/>
                                <c:inputTextarea id="confsess_comment__c" cntr="{!controller}" cols="45" rows="2" />
                            </apex:pageBlockSectionItem>
                            
                            <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_useconfig__c.inlineHelpText}">
                                <apex:outputLabel value="{!f.rstk__confsess_useconfig__c.label}"/>
