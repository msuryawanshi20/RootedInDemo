                </apex:pageBlockSectionItem>

                <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_opportunity__c.inlineHelpText}">
                    <apex:outputLabel value="{!f.rstk__confsess_opportunity__c.label}"/>
                    <apex:outputField value="{!o.rstk__confsess_opportunity__c}"/>
                </apex:pageBlockSectionItem>

                <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_useconfig__c.inlineHelpText}" rendered="{!isNew}">
                    <apex:outputLabel value="{!f.rstk__confsess_useconfig__c.label}"/>
                    <apex:inputField id="confsess_useconfig__c" value="{!o.rstk__confsess_useconfig__c}"/>
                </apex:pageBlockSectionItem>

                <apex:pageBlockSectionItem rendered="{!isNew}">
                    <apex:outputLabel value="{!f.rstk__confsess_confsess__c.label}"/>
                    <c:autocomplete id="confsess_confsess__c" cntr="{!controller}" query="select id, name from confsess__c where confsess_div__c = '{!o.rstk__confsess_div__c}'" width="200px"/>
                </apex:pageBlockSectionItem>

                <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_sopbk__c.inlineHelpText}" rendered="{!isNew}">
                    <apex:outputLabel value="{!f.rstk__confsess_sopbk__c.label}" style="width:120px"/>
                    <apex:selectlist id="confsess_sopbk__c" value="{!o.rstk__confsess_sopbk__c}" size="1" style="width:225">
                        <apex:selectOptions value="{!priceBooks}"/>
                    </apex:selectlist>
                </apex:pageBlockSectionItem>   

            </apex:pageBlockSection>

            <apex:pageBlockSection id="pbs_Main_View" showHeader="false" title="{!$Label.rstk__information}" columns="3" rendered="{!isView}">
                <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_div__c.inlineHelpText}" dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputLabel value="{!f.rstk__confsess_div__c.label}"/>
                    <apex:outputField value="{!o.rstk__confsess_div__c}"/>
                </apex:pageBlockSectionItem>
                
                <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_account__c.inlineHelpText}" dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputLabel value="{!f.rstk__confsess_account__c.label}"/>
                    <apex:outputField value="{!o.rstk__confsess_account__c}"/>
                </apex:pageBlockSectionItem>
                
                <apex:pageBlockSectionItem id="pbs_price1" helpText="{!f.rstk__confsess_price1__c.inlineHelpText}" rendered="{!cfg.showAltPrice1}" dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputLabel value="{!cfg.altPrice1Label}"/>
                    <apex:outputPanel id="op_price1">
                    	<apex:outputText id="price1" html-decimals="2" value="{!cfg.price1}" />
                        {!cfg.currencyUtil.maintenanceCurrencySymbol}
                    </apex:outputPanel>
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem rendered="{!!cfg.showAltPrice1}" dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputText value=""/>
                </apex:pageBlockSectionItem>
                
                
                <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_sessionid__c.inlineHelpText}" dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputLabel value="{!f.rstk__confsess_sessionid__c.label}"/>
                    <apex:outputField value="{!o.rstk__confsess_sessionid__c}"/>
                </apex:pageBlockSectionItem>
                
                <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_opportunity__c.inlineHelpText}" dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputLabel value="{!f.rstk__confsess_opportunity__c.label}"/>
                    <apex:outputField value="{!o.rstk__confsess_opportunity__c}"/>
                </apex:pageBlockSectionItem>

                <apex:pageBlockSectionItem id="pbs_price2" helpText="{!f.rstk__confsess_price2__c.inlineHelpText}" rendered="{!cfg.showAltPrice2}" dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputLabel value="{!cfg.altPrice2Label}"/>
                    <apex:outputPanel id="op_price2">
                        <apex:outputText id="price2" html-decimals="2" value="{!cfg.price2}" />
                        {!cfg.currencyUtil.maintenanceCurrencySymbol}
                    </apex:outputPanel>
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem rendered="{!isView && !cfg.showAltPrice2}" dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputText value=""/>
                </apex:pageBlockSectionItem>


                <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_soprod__c.inlineHelpText}" dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputLabel value="{!f.rstk__confsess_soprod__c.label}"/>
                    <apex:outputField value="{!o.rstk__confsess_soprod__c}"/>
                </apex:pageBlockSectionItem>

                <apex:pageBlockSectionItem dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputLabel value="{!$Label.rstk__datecreated}"/>
                    <apex:outputField value="{!o.Createddate}"/>
                </apex:pageBlockSectionItem>

                <apex:pageBlockSectionItem id="pbs_price" helpText="{!f.rstk__confsess_price__c.inlineHelpText}" rendered="{!cfg.showPrice}" dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputLabel value="{!f.rstk__confsess_price__c.label}"/>
                    <apex:outputPanel id="op_price">
                        <apex:outputText id="price" html-decimals="2" value="{!cfg.price}" />
                        {!cfg.currencyUtil.maintenanceCurrencySymbol}
                    </apex:outputPanel>
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem rendered="{!isView && !cfg.showPrice}" dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputText value=""/>
                </apex:pageBlockSectionItem>


                <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_socust__c.inlineHelpText}" dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputLabel value="{!f.rstk__confsess_socust__c.label}"/>
                    <apex:outputField value="{!o.rstk__confsess_socust__c}"/>
                </apex:pageBlockSectionItem>

                <apex:pageBlockSectionItem dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputLabel value="{!$Label.rstk__datelastmodified}"/>
                    <apex:outputPanel id="op_lastmoddte">
                        <apex:outputField value="{!o.Lastmodifieddate}"/>
                    </apex:outputPanel>
                </apex:pageBlockSectionItem>

                <apex:pageBlockSectionItem id="pbs_cost" helpText="{!f.rstk__confsess_cost__c.inlineHelpText}" rendered="{!cfg.showCost}" dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputLabel value="{!f.rstk__confsess_cost__c.label}"/>
                    <apex:outputPanel id="op_cost">
                        <apex:outputText id="cost" html-decimals="2" value="{!cfg.cost}" />
                    </apex:outputPanel>
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem rendered="{!isView && !cfg.showCost}" dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputLabel value=""/>
                    <apex:outputText value=""/>
                </apex:pageBlockSectionItem>

                <apex:pageBlockSectionItem dataStyle="width:20%" labelStyle="width:13%" rendered="{!customerSiteId != null}">
                    <apex:outputLabel value="{!$Label.rstk__customersite}"/>
                    <c:hoverDetail recId="{!customerSiteId}" recName="{!customerSiteName}"/>
                </apex:pageBlockSectionItem>

                <apex:pageBlockSectionItem dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputLabel value="{!$Label.rstk__status}"/>
                    <apex:outputPanel id="op_prodstatus2">
                        <apex:outputText value="{!configStatus}" rendered="{!o.rstk__confsess_dateout__c == null}"/>
                        <apex:outputPanel id="op_expstatus" rendered="{!o.rstk__confsess_dateout__c != null}">
                            <apex:outputPanel rendered="{!o.rstk__confsess_soline__c != null}">
                                <apex:outputText value="{!$Label.rstk__labelexportedtoquoteorderline} "/>
                                <apex:outputField value="{!o.rstk__confsess_soline__c}"/>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!o.rstk__confsess_soline__c == null && o.rstk__confsess_sohdr__c != null}">
                                <apex:outputText value="{!$Label.rstk__labelexportedtoquoteorder} "/>
                                <apex:outputField value="{!o.rstk__confsess_sohdr__c}"/>
                            </apex:outputPanel>
                            <apex:outputText value=" {!$Label.rstk__labelon} " rendered="{!o.rstk__confsess_soline__c != null || o.rstk__confsess_sohdr__c != null}"/>
                            <apex:outputField value="{!o.rstk__confsess_dateout__c}" rendered="{!o.rstk__confsess_soline__c != null || o.rstk__confsess_sohdr__c != null}"/>
                            <apex:outputText value="{!configStatus}" rendered="{!o.rstk__confsess_expicitemsrl__c = true}"/>
                        </apex:outputPanel>
                    </apex:outputPanel>
                </apex:pageBlockSectionItem>
                
                <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_duedate__c.inlineHelpText}" dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputLabel value="{!f.rstk__confsess_duedate__c.label}"/>
                    <c:inputField id="confsess_duedate__c" cntr="{!controller}" tabIndex="31"/>
                </apex:pageBlockSectionItem>
                
                <apex:pageBlockSectionItem id="pbs_totcost" helpText="{!f.rstk__confsess_totdepcompprice__c.inlineHelpText}" dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputLabel value="{!f.rstk__confsess_totdepcompprice__c.label}"/>
                    <apex:outputPanel id="op_totcost">
                        <apex:outputText id="totcost" html-decimals="2" value="{!cfg.totdepcompprice}" />
                    </apex:outputPanel>
                </apex:pageBlockSectionItem>
                
                <apex:pageBlockSectionItem rendered="{!customerSiteId != null}" dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputLabel value=""/>
                    <apex:outputText value=""/>
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem rendered="{!customerSiteId != null}" dataStyle="width:20%" labelStyle="width:13%">
                    <apex:outputLabel value=""/>
                    <apex:outputText value=""/>
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem dataStyle="width:20%" labelStyle="width:13%" rendered="{!customerSiteId != null}">
                    <apex:outputLabel value="Dealer Name"/>
                    <c:hoverDetail recId="{!dealerId}" recName="{!dealerName}"/>
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem dataStyle="width:20%" labelStyle="width:13%" rendered="{!customerSiteId != null}"><apex:outputText value=""/></apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem dataStyle="width:20%" labelStyle="width:13%" rendered="{!customerSiteId != null}"><apex:outputText value=""/></apex:pageBlockSectionItem>
                
            </apex:pageBlockSection>
            
            <apex:pageBlockSection id="pbs_Advanced" showHeader="true" title="Advanced" columns="1" rendered="{!!isNew}">
            
                <apex:tabPanel switchType="client" selectedtab="tab_general" id="theTabPanel" headerSpacing="1">
                    <apex:tab label="{!$Label.rstk__general}" name="tab_general" id="tab_general" labelWidth="120">
                        <apex:pageBlockSection id="pbs_general" columns="2">
                        
                            <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_desc__c.inlineHelpText}">
                                <apex:outputLabel value="{!f.rstk__confsess_desc__c.label}"/>
                                <c:inputField id="confsess_desc__c" cntr="{!controller}"/>
                            </apex:pageBlockSectionItem>
                                                    
                            <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_installcust__c.inlineHelpText}">
                                 <apex:outputLabel value="{!f.rstk__confsess_installcust__c.label}"/>
                                 <apex:outputPanel id="op_installcust">
                                 	<c:largeSelectList id="confsess_installcust__c" cntr="{!controller}" onchange="installCustChanged(this.value);" options="{!installSocusts}"/>
                                 </apex:outputPanel>
                            </apex:pageBlockSectionItem>
                            
                            <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_discpct__c.inlineHelpText}">
                                <apex:outputLabel value="{!f.rstk__confsess_discpct__c.label}"/>
                                <c:inputField id="confsess_discpct__c" cntr="{!controller}"/>
                            </apex:pageBlockSectionItem>
                            
                            <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_instaddr__c.inlineHelpText}">
                                 <apex:outputLabel value="{!f.rstk__confsess_instaddr__c.label}"/>
                                 <apex:outputPanel id="op_instaddr">
                                    <c:largeSelectList id="confsess_instaddr__c" cntr="{!controller}" options="{!installAdresses}"/>
                                 </apex:outputPanel>
                            </apex:pageBlockSectionItem>
                            
                            <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_discmethod__c.inlineHelpText}">
                                <apex:outputLabel value="{!f.rstk__confsess_discmethod__c.label}"/>
                                <c:selectList id="confsess_discmethod__c" cntr="{!controller}" options="{!DiscountMethods}"/>
                            </apex:pageBlockSectionItem>
                            
                            <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_staddr__c.inlineHelpText}">
                                 <apex:outputLabel value="{!f.rstk__confsess_staddr__c.label}"/>
