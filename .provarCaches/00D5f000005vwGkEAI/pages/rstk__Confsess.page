                            </apex:pageBlockSectionItem>

                            <apex:outputField value="{!o.rstk__confsess_confsess__c}"/>

                            <apex:pageBlockSectionItem helpText="{!f.rstk__confsess_sopbk__c.inlineHelpText}">
