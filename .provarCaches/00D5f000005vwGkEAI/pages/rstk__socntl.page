        var dfltInvdiv = {!dfltInvdivJSON};
        var dfltComcod = {!dfltComcodJSON};
        
        function closeEmailTmplClonePopup() {
            jQuery(".close").click();
            refreshEmailTemplatesTab();
        }
        jQuery(document).ready(function() {
            jQuery("button[rel*=win]").overlay({
                mask: {
                    color: '#ebecff',
                    loadSpeed: 200,
                    opacity: 0.8
                },
                
                closeOnClick: false,
                
                onBeforeLoad: function(e) {
                    jQuery('#winVFPage').block({
                        message: '',
                        centerX: true, 
                        centerY: true,
                        css: { 
                            border: 'none', 
                            padding: '15px', 
                            cursor: 'wait',
                            opacity: .5, 
                            textAlign: 'center'
                        }
                    });
                    
                    var winId = e.target.getTrigger().attr('class').substring(3);
                    var templateId = jQuery(e.target.getTrigger().closest('span')[0].previousSibling).find('select').val()
                    var content = jQuery("#"+winId+"Frame");
                    if (content.length>0) {
                        if (winId == 'CloneEmailTemplate') {
                            content[0].src = "/apex/CloneEmailTemplate?srcid="+templateId;
                        }
                    
                        content.load(function() {
                            jQuery('#winVFPage').unblock();
                        });
                    }
                },
                onClose: function(e) {
                    var winId = e.target.getTrigger().attr('class').substring(3);
                    var content = jQuery("#"+winId+"Frame");
                    if (content.length>0) content[0].src = 'about:blank';
                }
            });
            
            if (dfltSocomm && jQuery('span[id*=dfltsocomm]').length > 0)
                setFieldText(jQuery('span[id*=dfltsocomm]')[0], dfltSocomm);
            if (dfltInvdiv && jQuery('span[id*=dfltinvdiv]').length > 0)
                setFieldText(jQuery('span[id*=dfltinvdiv]')[0], dfltInvdiv);
            if (dfltComcod && jQuery('span[id*=dfltcomcod]').length > 0)
                setFieldText(jQuery('span[id*=dfltcomcod]')[0], dfltComcod);
        });
    </script>

