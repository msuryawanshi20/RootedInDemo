    function QuantityDetails(rowId){
        soptId = rowId;
        jQuery(".quantityDetails").click();
    }
    
    function jqueryOnReady() {
        initHoverPopups(jQuery("#bodyTable"), {manualClose:false, displayUp:true, onClose:{}});
         jQuery("button[rel*=win]").overlay({
            mask: {
                color: '#ebecff',
                loadSpeed: 200,
                opacity: 0.8
            },
            
            closeOnClick: false,
            
            onBeforeLoad: function(e) {
                jQuery('#winVFPage').block({
                    message: '',
                    centerX: true, 
                    centerY: true,
                    css: { 
                        border: 'none', 
                        padding: '15px', 
                        cursor: 'wait',
                        opacity: .5, 
                        textAlign: 'center'
                    }
                });
                
                var winId = e.target.getTrigger().attr('class');
                var content = jQuery("#"+winId+"Frame");
                if (content.length > 0) {
                    if (winId == 'quantityDetails') {
                        content[0].src = "/apex/quantityDetailpopupPage?id=" +soptId ;
                    } 
                
                    content.load(function() {
                        jQuery('#winVFPage').unblock();
                    });
                }
            },
            onClose: function(e){
                var winId = e.target.getTrigger().attr('class');
                var content = jQuery("#"+winId+"Frame");
                if (content.length > 0) { 
                    content[0].src = 'about:blank'; 
                }
              //  window.location.replace(window.location);
            }
        });  
    }      
    
    function validateGrids() {
        if(isNew) {
            return true;
        } else {
            if(typeof confgroupps == 'undefined' || confgroupps == null) return true;
            if(!confgroupps.isDirty()) {
                return true;
            } else {
                var crfm = confirm("{!$Label.rstk__unsavedchangesinconfiggrptab_savechangesbeforesavingheader_ignorechanges}");
                if(crfm) {
                    return true;
                } else {
                    return false;
                }
            }
        }
    }
    jQuery(function() { setTimeout(function() { enableDisableEntitlement(); }, 1500); });
    </script>
    
