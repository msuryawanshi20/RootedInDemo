                <apex:tab label="{!$Label.rstk__pagetab_soprod_productpricebydate}" name="tab_ProdPriceByDate" id="tab_ProdPriceByDate">
                    <c:PageableGrid width="1150" jsController="productPrice" data="{!productPricingDetails}" pageSize="5" editable="{!isView}" ispopup="false" disableSave="{!!isView}"/>
                    <script>
                        productPrice.onGridLoad = function() {
                            self = this;
                            this.getVisibleRows().each(function (index, el) {
                               var row = jQuery(this);
                               var rowId = self.getRowValue(row, 'Id');
                               if (isEmpty(rowId)) rowId = productPrice.rowIds[index];
                                if (!isEmpty(rowId)) {
                                        self.getRowObj(row, 'sopt_frstdate__c').attr('disabled', true);
                                        self.getRowObj(row, 'quantity__g').html('<button type="button" class="btn" onclick="QuantityDetails(\'' + rowId + '\');">Quantity Detail</button>');
                                    }
                           });  
                        }
                        
                        productPrice.validate = function() {
                            self = this;
                            var isvalid = true;
                            this.getVisibleRows().each(function (index, el) {
                                var row = jQuery(this);
                                var price = productPrice.getRowValue(row, 'sopt_price__c');
                                var firstDate = productPrice.getRowValue(row, 'sopt_frstdate__c');
                                var lastDate = productPrice.getRowValue(row, 'sopt_lastdate__c');
                                
                                if(!price || parseInt(price) < 0) {
                                    alert("{!$Label.rstk__pricecantbelt0}"); 
                                    productPrice.getRowObj(row, 'sopt_price__c').focus();
                                    isvalid = false;
                                    return false;
                                }
                                
                                if (!firstDate) {
                                    alert("{!$Label.rstk__firsteffectivedate}"); 
                                    productPrice.getRowObj(row, 'sopt_frstdate__c').focus();
                                    isvalid = false;
                                    return false;
                                }
                                
                                if (!lastDate) {
                                    alert("{!$Label.rstk__lasteffectivedate}"); 
                                    productPrice.getRowObj(row, 'sopt_lastdate__c').focus();
                                    isvalid = false;
                                    return false;
                                }
                            });
                             return isvalid;
                         } 
                        
                        
                        productPrice.entryChanged = function(row, rowNum, name, value) {
                            if (name == 'sopt_lastdate__c') {
                                var firstDate = productPrice.getRowValue(row, 'sopt_frstdate__c');
                                var lastDate = productPrice.getRowValue(row, 'sopt_lastdate__c');
                                if (firstDate != null && lastDate != null) {
                                    verifyDate(firstDate,lastDate,row);
                                }
                            }
                        }
                        
                         function verifyDate(firstDate,lastDate,row){
                            Visualforce.remoting.Manager.invokeAction(  
                                '{!$RemoteAction.ControllerExtnSoprod.verifyDate}', //NameSpace  
                                firstDate,lastDate,   
                                function(result, event){
                                    if (event.status) {
                                        if(result){
                                            alert("{!$Label.rstk__lstefctdtcntbelessthnfstefctdte}");
                                            productPrice.setRowValue(row, "sopt_lastdate__c", '');
                                            return true;
                                        }
                                    } else if (event.type === 'exception') {
                                        alert('Exception: '+event.message);
                                    } else {
                                        alert("{!$Label.rstk__unexpectedstatus}", event.message);
                                    }
                                }, {escape:false}
                            );
                        }
                    </script>
                
                </apex:tab>
                
