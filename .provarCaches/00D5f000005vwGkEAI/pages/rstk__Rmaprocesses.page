            		<apex:inputField id="transfertoscrap__c" value="{!header.rstk__rmarcpt_transfertoscrap__c}"/>
            	</apex:pageBlockSectionItem>
            	<apex:pageBlockSectionItem />
            	
            	<apex:pageBlockSectionItem />
            	<apex:pageBlockSectionItem />
            	<apex:pageBlockSectionItem helpText="{!f_header.rstk__rmarcpt_reverse__c.inlineHelpText}">
