            		 <!-- <apex:selectList id="transferfromlocid__c" value="{!header.rstk__rmarcpt_transferfromlocid__c}" multiselect="false" size="1" style="width:175px;">
                         <apex:selectOptions value="{!sylocids}"/>
                     </apex:selectList> -->
