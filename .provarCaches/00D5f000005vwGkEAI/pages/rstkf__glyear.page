    <script>
        function initializeP01StartDate(fiscalYear,glyearCompany){
            Visualforce.remoting.Manager.invokeAction(  
                '{!$RemoteAction.ControllerExtnGlyear.initializeP01StartDate}', //NameSpace  
                fiscalYear,
                glyearCompany,  
                function(result, event){
                    if (event.status) {
                        populateDefaultDate(result);
                    } else if (event.type === 'exception') {
                        alert("{!$Label.Exception}", event.message);
                    } else {
                        alert("{!$Label.UnexpectedStatus}", event.message);
                    }
                }, {escape:false}
            );
        }
        
        function initializeStartDate(endDateFldId,startDatefieldID,fieldLabel,endDateValue){
            Visualforce.remoting.Manager.invokeAction(  
                '{!$RemoteAction.ControllerExtnGlyear.initializeStartDate}', //NameSpace  
                endDateValue,
                function(result, event){
                    if (event.status) {
                        document.getElementById(startDatefieldID).value = result;
                    } else if (event.type === 'exception') {
                        alert(event.message + ' for '+ fieldLabel);
                        document.getElementById(endDateFldId).value = '';
                        document.getElementById(endDateFldId).focus();
                    } else {
                        alert("{!$Label.UnexpectedStatus}", event.message);
                    }
                }, {escape:false}
            );
        }
        
        function verifyGapInDates(endDateValue,startDateValue,startDatefieldID,endDtFldLabel,startDtFldLabel){
            Visualforce.remoting.Manager.invokeAction(  
                '{!$RemoteAction.ControllerExtnGlyear.verifyGapInDates}', //NameSpace  
                endDateValue,
                startDateValue,
                function(result, event){
                    if (event.status) {
                        if(result){
                            alert(Label_nogapsbetweentwodates,[endDtFldLabel,startDtFldLabel,startDtFldLabel,endDtFldLabel]);
                            document.getElementById(startDatefieldID).value = '';
                            document.getElementById(startDatefieldID).focus();
                        }
                    } else if (event.type === 'exception') {
                        alert(event.message + ' for '+ startDtFldLabel);
                        document.getElementById(startDatefieldID).value = '';
                        document.getElementById(startDatefieldID).focus();
                    } else {
                        alert("{!$Label.UnexpectedStatus}", event.message);
                    }
                }, {escape:false}
            );
        }

    </script> 
