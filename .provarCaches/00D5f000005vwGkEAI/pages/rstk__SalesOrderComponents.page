	   		showError('The value can\'t be more than 999.99');
	   		value = 0;
	   		this.setRowValue(row, name, value);
	   		this.getRowObj(row, name).focus();
	   	}
   	}
   }
   
   if (name == 'soconfig_reqdate__c')
   {
   	var theDate = sfffStringToDate(value);
